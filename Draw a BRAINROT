-- –ê–ù–ò–ú–ê–¶–ò–ò –ò –≠–§–§–ï–ö–¢–´
local function tween(frame, properties, time, easingStyle, easingDirection)
    local tweenInfo = TweenInfo.new(time or 0.3, easingStyle or Enum.EasingStyle.Quart, easingDirection or Enum.EasingDirection.Out)
    local tween = TweenService:Create(frame, tweenInfo, properties)
    tween:Play()
    return tween
end

-- Hover —ç—Ñ—Ñ–µ–∫—Ç –¥–ª—è –∫–Ω–æ–ø–æ–∫
local function addHover(button)
    button.MouseEnter:Connect(function()
        tween(button, {Size = button.Size + UDim2.new(0, 8, 0, 4), BackgroundColor3 = Color3.fromRGB(60, 60, 90)}, 0.2)
    end)
    button.MouseLeave:Connect(function()
        tween(button, {Size = button.Size - UDim2.new(0, 8, 0, 4), BackgroundColor3 = Color3.fromRGB(40, 40, 60)}, 0.2)
    end)
end

addHover(WallhackButton)
addHover(ESPButton)
addHover(CloseButton)

-- –ö–ª—é—á –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è (Insert)
UserInputService.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.Insert then
        MenuOpen = not MenuOpen
        MainFrame.Visible = MenuOpen
        
        if MenuOpen then
            tween(MainFrame, {Size = UDim2.new(0, 400, 0, 300), Position = UDim2.new(0.5, -200, 0.5, -150)}, 0.5, Enum.EasingStyle.Back)
        else
            tween(MainFrame, {Size = UDim2.new(0, 0, 0, 0), Position = UDim2.new(0.5, 0, 0.5, 0)}, 0.4, Enum.EasingStyle.Quart)
        end
    end
end)

-- Wallhack —Ñ—É–Ω–∫—Ü–∏—è
local function toggleWallhack()
    WallhackEnabled = not WallhackEnabled
    WallhackButton.Text = WallhackEnabled and "üëÅÔ∏è WALLHACK: ON" or "üëÅÔ∏è WALLHACK: OFF"
    WallhackButton.BackgroundColor3 = WallhackEnabled and Color3.fromRGB(0, 255, 100) or Color3.fromRGB(40, 40, 60)
    
    for _, player in pairs(Players:GetPlayers()) do
        if player.Character and player.Character:FindFirstChild("Head") then
            for _, part in pairs(player.Character:GetChildren()) do
                if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
                    part.CanCollide = not WallhackEnabled
                    part.Transparency = WallhackEnabled and 0.7 or 0
                end
            end
        end
    end
end

WallhackButton.MouseButton1Click:Connect(toggleWallhack)

-- Walkspeed —Å–ª–∞–π–¥–µ—Ä
local dragging = false
local minSpeed, maxSpeed = 16, 100

WalkspeedSlider.MouseButton1Down:Connect(function()
    dragging = true
end)

UserInputService.InputChanged:Connect(function(input)
    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
        local mouse = LocalPlayer:GetMouse()
        local relativeX = math.clamp((mouse.X - WalkspeedSlider.AbsolutePosition.X) / WalkspeedSlider.AbsoluteSize.X, 0, 1)
        WalkspeedValue = math.floor(minSpeed + (maxSpeed - minSpeed) * relativeX)
        WalkspeedValueLabel.Text = tostring(WalkspeedValue)
        WalkspeedSlider.Size = UDim2.new(relativeX, 0, 1, 0)
        
        if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
            LocalPlayer.Character.Humanoid.WalkSpeed = WalkspeedValue
        end
    end
end)

UserInputService.InputEnded:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = false
    end
end)

-- ESP —Ñ—É–Ω–∫—Ü–∏—è
local ESPEnabled = false
local ESPObjects = {}

local function toggleESP()
    ESPEnabled = not ESPEnabled
    ESPButton.Text = ESPEnabled and "üéØ ESP: ON" or "üéØ ESP: OFF"
    ESPButton.BackgroundColor3 = ESPEnabled and Color3.fromRGB(255, 165, 0) or Color3.fromRGB(40, 40, 60)
    
    if ESPEnabled then
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character and player.Character:FindFirstChild("Head") then
                local esp = Instance.new("BillboardGui")
                esp.Name = "ESP_" .. player.Name
                esp.Parent = player.Character.Head
                esp.Size = UDim2.new(0, 100, 0, 50)
                esp.StudsOffset = Vector3.new(0, 3, 0)
                
                local nameLabel = Instance.new("TextLabel")
                nameLabel.Parent = esp
                nameLabel.BackgroundTransparency = 1
                nameLabel.Size = UDim2.new(1, 0, 1, 0)
                nameLabel.Font = Enum.Font.GothamBold
                nameLabel.Text = player.Name
                nameLabel.TextColor3 = Color3.fromRGB(255, 0, 150)
                nameLabel.TextStrokeTransparency = 0
                nameLabel.TextScaled = true
                
                ESPObjects[player] = esp
            end
        end
    else
        for _, esp in pairs(ESPObjects) do
            if esp then esp:Destroy() end
        end
        ESPObjects = {}
    end
end

ESPButton.MouseButton1Click:Connect(toggleESP)

-- –ó–∞–∫—Ä—ã—Ç–∏–µ –º–µ–Ω—é
CloseButton.MouseButton1Click:Connect(function()
    MenuOpen = false
    tween(MainFrame, {Size = UDim2.new(0, 0, 0, 0)}, 0.3)
end)

-- –ê–≤—Ç–æ-walkspeed –ø—Ä–∏ —Ä–µ—Å–ø–∞–≤–Ω–µ
LocalPlayer.CharacterAdded:Connect(function(character)
    local humanoid = character:WaitForChild("Humanoid")
    humanoid.WalkSpeed = WalkspeedValue
end)

-- –ü—É–ª—å—Å–∏—Ä—É—é—â–∏–π —ç—Ñ—Ñ–µ–∫—Ç –∑–∞–≥–æ–ª–æ–≤–∫–∞
spawn(function()
    while true do
        tween(Title, {TextTransparency = 0.3}, 1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
        wait(1)
        tween(Title, {TextTransparency = 0}, 1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut)
        wait(1)
    end
end)

print("üß† STIDEV | Draw a BRAINROT Menu LOADED! –ù–∞–∂–º–∏ INSERT –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è!")
