updateButton.MouseButton1Click:Connect(function()
    updateButton.Text = "–û–ë–ù–û–í–õ–Ø–ï–ú..."
    
    -- –ò–º–∏—Ç–∞—Ü–∏—è –ø—Ä–æ—Ü–µ—Å—Å–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
    spawn(function()
        for i = 1, 100 do
            updateButton.Text = "üì• –ó–∞–≥—Ä—É–∑–∫–∞: " .. i .. "%"
            wait(0.01)
        end
        
        updateButton.Text = "‚úÖ –û–ë–ù–û–í–õ–ï–ù–û!"
        updateButton.BackgroundColor3 = Color3.fromRGB(0, 100, 0)
        
        -- –≠—Ñ—Ñ–µ–∫—Ç –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
        local particles = createParticleEffect(updateButton)
        wait(1)
        particles:Destroy()
        
        wait(2)
        updateButton.Text = "üîÑ –û–ë–ù–û–í–ò–¢–¨ –ß–ò–¢–´"
        updateButton.BackgroundColor3 = Color3.fromRGB(34, 139, 34)
    end)
end)

