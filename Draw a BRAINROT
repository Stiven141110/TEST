-- 1. WallHack (—Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã)
local wallhackEnabled = false
local originalParts = {}
local wallhackButton = Instance.new("TextButton")
wallhackButton.Name = "WallHackBtn"
wallhackButton.Size = UDim2.new(1, 0, 0, 50)
wallhackButton.BackgroundColor3 = Color3.fromRGB(30, 144, 255)
wallhackButton.Text = "üöÄ WALLHACK: –í–´–ö–õ"
wallhackButton.TextColor3 = Color3.white
wallhackButton.Font = Enum.Font.GothamBold
wallhackButton.TextSize = 20
wallhackButton.Parent = ScrollingFrame

wallhackButton.MouseButton1Click:Connect(function()
    wallhackEnabled = not wallhackEnabled
    
    if wallhackEnabled then
        wallhackButton.Text = "üöÄ WALLHACK: –í–ö–õ"
        wallhackButton.BackgroundColor3 = Color3.fromRGB(50, 205, 50)
        
        -- –î–µ–ª–∞–µ–º —Å—Ç–µ–Ω—ã –ø—Ä–æ–∑—Ä–∞—á–Ω—ã–º–∏
        for _, part in pairs(workspace:GetDescendants()) do
            if part:IsA("BasePart") and part.Name:lower():find("wall") then
                originalParts[part] = {
                    Transparency = part.Transparency,
                    CanCollide = part.CanCollide
                }
                part.Transparency = 0.8
                part.CanCollide = false
            end
        end
    else
        wallhackButton.Text = "üöÄ WALLHACK: –í–´–ö–õ"
        wallhackButton.BackgroundColor3 = Color3.fromRGB(30, 144, 255)
        
        -- –í–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º —Å—Ç–µ–Ω—ã
        for part, properties in pairs(originalParts) do
            if part and part.Parent then
                part.Transparency = properties.Transparency
                part.CanCollide = properties.CanCollide
            end
        end
        originalParts = {}
    end
    
    -- –≠—Ñ—Ñ–µ–∫—Ç –ø—Ä–∏ –Ω–∞–∂–∞—Ç–∏–∏
    local effect = createParticleEffect(wallhackButton)
    wait(0.3)
    effect:Destroy()
end)

-- 2. WalkSpeed –∫–æ–Ω—Ç—Ä–æ–ª–ª–µ—Ä
local walkspeedValue = 16
local walkspeedButton = Instance.new("TextButton")
walkspeedButton.Name = "WalkSpeedBtn"
walkspeedButton.Size = UDim2.new(1, 0, 0, 50)
walkspeedButton.BackgroundColor3 = Color3.fromRGB(255, 69, 0)
walkspeedButton.Text = "‚ö° WALKSPEED: " .. walkspeedValue
walkspeedButton.TextColor3 = Color3.white
walkspeedButton.Font = Enum.Font.GothamBold
walkspeedButton.TextSize = 20
walkspeedButton.Parent = ScrollingFrame

local speedSlider = Instance.new("Frame")
speedSlider.Name = "Slider"
speedSlider.Size = UDim2.new(1, -20, 0, 30)
speedSlider.Position = UDim2.new(0, 10, 0, 110)
speedSlider.BackgroundColor3 = Color3.fromRGB(40, 40, 60)
speedSlider.Visible = false
speedSlider.Parent = FunctionsFrame

local speedFill = Instance.new("Frame")
speedFill.Name = "Fill"
speedFill.Size = UDim2.new((walkspeedValue - 16) / 100, 0, 1, 0)
speedFill.BackgroundColor3 = Color3.fromRGB(0, 255, 127)
speedFill.BorderSizePixel = 0
speedFill.Parent = speedSlider

local speedText = Instance.new("TextLabel")
speedText.Name = "SpeedText"
speedText.Size = UDim2.new(1, 0, 1, 0)
speedText.BackgroundTransparency = 1
speedText.Text = "–°–∫–æ—Ä–æ—Å—Ç—å: " .. walkspeedValue
speedText.TextColor3 = Color3.white
speedText.Font = Enum.Font.Gotham
speedText.TextSize = 16
speedText.Parent = speedSlider

walkspeedButton.MouseButton1Click:Connect(function()
    speedSlider.Visible = not speedSlider.Visible
end)

speedSlider.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        local connection
        connection = RunService.RenderStepped:Connect(function()
            local mouse = UserInputService:GetMouseLocation()
            local absolutePosition = speedSlider.AbsolutePosition.X
            local absoluteSize = speedSlider.AbsoluteSize.X
            
            local relativeX = math.clamp(mouse.X - absolutePosition, 0, absoluteSize)
            local percentage = relativeX / absoluteSize
            
            walkspeedValue = math.floor(16 + percentage * 100)
            speedFill.Size = UDim2.new(percentage, 0, 1, 0)
            speedText.Text = "–°–∫–æ—Ä–æ—Å—Ç—å: " .. walkspeedValue
            walkspeedButton.Text = "‚ö° WALKSPEED: " .. walkspeedValue
            
            -- –ü—Ä–∏–º–µ–Ω—è–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å –ø–µ—Ä—Å–æ–Ω–∞–∂—É
            local player = Players.LocalPlayer
            if player and player.Character and player.Character:FindFirstChild("Humanoid") then
                player.Character.Humanoid.WalkSpeed = walkspeedValue
            end
        end)
        
        UserInputService.InputEnded:Connect(function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 then
                connection:Disconnect()
            end
        end)
    end
end)

-- 3. God Mode
local godModeEnabled = false
local godModeButton = Instance.new("TextButton")
godModeButton.Name = "GodModeBtn"
godModeButton.Size = UDim2.new(1, 0, 0, 50)
godModeButton.BackgroundColor3 = Color3.fromRGB(220, 20, 60)
godModeButton.Text = "üõ°Ô∏è GOD MODE: –í–´–ö–õ"
godModeButton.TextColor3 = Color3.white
godModeButton.Font = Enum.Font.GothamBold
godModeButton.TextSize = 20
godModeButton.Parent = ScrollingFrame

godModeButton.MouseButton1Click:Connect(function()
    godModeEnabled = not godModeEnabled
    
    if godModeEnabled then
        godModeButton.Text = "üõ°Ô∏è GOD MODE: –í–ö–õ"
        godModeButton.BackgroundColor3 = Color3.fromRGB(50, 205, 50)
        
        local player = Players.LocalPlayer
        if player and player.Character then
            -- –ó–∞—â–∏—Ç–∞ –æ—Ç —É—Ä–æ–Ω–∞
            for _, part in pairs(player.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                    part.Transparency = 0.3
                end
            end
        end
    else
        godModeButton.Text = "üõ°Ô∏è GOD MODE: –í–´–ö–õ"
        godModeButton.BackgroundColor3 = Color3.fromRGB(220, 20, 60)
        
        local player = Players.LocalPlayer
        if player and player.Character then
            -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º –Ω–æ—Ä–º–∞–ª—å–Ω—ã–µ —Å–≤–æ–π—Å—Ç–≤–∞
            for _, part in pairs(player.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = true
                    part.Transparency = 0
                end
            end
        end
    end
end)

-- 4. Fly Mode
local flyEnabled = false
local flyButton = Instance.new("TextButton")
flyButton.Name = "FlyBtn"
flyButton.Size = UDim2.new(1, 0, 0, 50)
flyButton.BackgroundColor3 = Color3.fromRGB(148, 0, 211)
flyButton.Text = "ü¶Ö FLY MODE: –í–´–ö–õ"
flyButton.TextColor3 = Color3.white
flyButton.Font = Enum.Font.GothamBold
flyButton.TextSize = 20
flyButton.Parent = ScrollingFrame

flyButton.MouseButton1Click:Connect(function()
    flyEnabled = not flyEnabled
    
    if flyEnabled then
        flyButton.Text = "ü¶Ö FLY MODE: –í–ö–õ"
        flyButton.BackgroundColor3 = Color3.fromRGB(50, 205, 50)
        
        -- –ê–∫—Ç–∏–≤–∞—Ü–∏—è –ø–æ–ª–µ—Ç–∞
        local player = Players.LocalPlayer
        if player and player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.PlatformStand = true
            end
        end
        
        -- –°–∏—Å—Ç–µ–º–∞ —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –ø–æ–ª–µ—Ç–æ–º
        local flyConnection
        flyConnection = RunService.RenderStepped:Connect(function()
            if not flyEnabled then
                flyConnection:Disconnect()
                return
            end
            
            local player = Players.LocalPlayer
            if player and player.Character then
                local root = player.Character:FindFirstChild("HumanoidRootPart")
                if root then
                    local velocity = Vector3.new(0, 0, 0)
                    
                    if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                        velocity = velocity + (root.CFrame.LookVector * 50)
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                        velocity = velocity - (root.CFrame.LookVector * 50)
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                        velocity = velocity - (root.CFrame.RightVector * 50)
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                        velocity = velocity + (root.CFrame.RightVector * 50)
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                        velocity = velocity + Vector3.new(0, 50, 0)
                    end
                    if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                        velocity = velocity - Vector3.new(0, 50, 0)
                    end
                    
                    root.Velocity = velocity
                end
            end
        end)
    else
        flyButton.Text = "ü¶Ö FLY MODE: –í–´–ö–õ"
        flyButton.BackgroundColor3 = Color3.fromRGB(148, 0, 211)
        
        local player = Players.LocalPlayer
        if player and player.Character then
            local humanoid = player.Character:FindFirstChild("Humanoid")
            if humanoid then
                humanoid.PlatformStand = false
            end
        end
    end
end)

-- 5. Auto-Farm –æ—á–∫–æ–≤
local autoFarmEnabled = false
local autoFarmButton = Instance.new("TextButton")
autoFarmButton.Name = "AutoFarmBtn"
autoFarmButton.Size = UDim2.new(1, 0, 0, 50)
autoFarmButton.BackgroundColor3 = Color3.fromRGB(255, 140, 0)
autoFarmButton.Text = "üí∞ AUTO-FARM: –í–´–ö–õ"
autoFarmButton.TextColor3 = Color3.white
autoFarmButton.Font = Enum.Font.GothamBold
autoFarmButton.TextSize = 20
autoFarmButton.Parent = ScrollingFrame

autoFarmButton.MouseButton1Click:Connect(function()
    autoFarmEnabled = not autoFarmEnabled
    
    if autoFarmEnabled then
        autoFarmButton.Text = "üí∞ AUTO-FARM: –í–ö–õ"
        autoFarmButton.BackgroundColor3 = Color3.fromRGB(50, 205, 50)
        
        -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–π —Å–±–æ—Ä –æ—á–∫–æ–≤
        spawn(function()
            while autoFarmEnabled do
                wait(0.5)
                
                -- –ü–æ–∏—Å–∫ –±–ª–∏–∂–∞–π—à–∏—Ö –æ–±—ä–µ–∫—Ç–æ–≤ –¥–ª—è —Å–±–æ—Ä–∞
                local player = Players.LocalPlayer
                if player and player.Character then
                    local root = player.Character:FindFirstChild("HumanoidRootPart")
                    if root then
                        for _, obj in pairs(workspace:GetChildren()) do
                            if obj.Name:lower():find("coin") or obj.Name:lower():find("point") then
                                if (root.Position - obj.Position).Magnitude < 50 then
                                    -- –¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º—Å—è –∫ –æ–±—ä–µ–∫—Ç—É
                                    root.CFrame = CFrame.new(obj.Position + Vector3.new(0, 3, 0))
                                    wait(0.1)
                                end
                            end
                        end
                    end
                end
            end
        end)
    else
        autoFarmButton.Text = "üí∞ AUTO-FARM: –í–´–ö–õ"
        autoFarmButton.BackgroundColor3 = Color3.fromRGB(255, 140, 0)
    end
end)

-- 6. ESP –¥–ª—è –∏–≥—Ä–æ–∫–æ–≤
local espEnabled = false
local espButton = Instance.new("TextButton")
espButton.Name = "ESPButton"
espButton.Size = UDim2.new(1, 0, 0, 50)
espButton.BackgroundColor3 = Color3.fromRGB(0, 191, 255)
espButton.Text = "üëÅÔ∏è PLAYER ESP: –í–´–ö–õ"
espButton.TextColor3 = Color3.white
espButton.Font = Enum.Font.GothamBold
espButton.TextSize = 20
espButton.Parent = ScrollingFrame

espButton.MouseButton1Click:Connect(function()
    espEnabled = not espEnabled
    
    if espEnabled then
        espButton.Text = "üëÅÔ∏è PLAYER ESP: –í–ö–õ"
        espButton.BackgroundColor3 = Color3.fromRGB(50, 205, 50)
        
        -- –°–æ–∑–¥–∞–Ω–∏–µ ESP –¥–ª—è –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= Players.LocalPlayer and player.Character then
                local highlight = Instance.new("Highlight")
                highlight.Name = "STIDEV_ESP"
                highlight.FillColor = Color3.fromRGB(255, 0, 0)
                highlight.OutlineColor = Color3.fromRGB(255, 255, 0)
                highlight.FillTransparency = 0.5
                highlight.OutlineTransparency = 0
                highlight.Parent = player.Character
                
                -- –¢–µ–∫—Å—Ç —Å –∏–º–µ–Ω–µ–º –∏–≥—Ä–æ–∫–∞
                local billboard = Instance.new("BillboardGui")
                billboard.Name = "ESP_Name"
                billboard.Size = UDim2.new(0, 200, 0, 50)
                billboard.StudsOffset = Vector3.new(0, 3, 0)
                billboard.AlwaysOnTop = true
                billboard.Parent = player.Character:WaitForChild("Head")
                
                local nameLabel = Instance.new("TextLabel")
                nameLabel.Size = UDim2.new(1, 0, 1, 0)
                nameLabel.BackgroundTransparency = 1
                nameLabel.Text = player.Name
                nameLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
                nameLabel.Font = Enum.Font.GothamBold
                nameLabel.TextSize = 18
                nameLabel.Parent = billboard
            end
        end
    else
        espButton.Text = "üëÅÔ∏è PLAYER ESP: –í–´–ö–õ"
        espButton.BackgroundColor3 = Color3.fromRGB(0, 191, 255)
        
        -- –£–¥–∞–ª–µ–Ω–∏–µ ESP
        for _, player in pairs(Players:GetPlayers()) do
            if player.Character then
                for _, child in pairs(player.Character:GetDescendants()) do
                    if child.Name == "STIDEV_ESP" or child.Name == "ESP_Name" then
                        child:Destroy()
                    end
                end
            end
        end
    end
end)

