-- –ê–Ω–∏–º–∞—Ü–∏—è –∫–Ω–æ–ø–æ–∫
for _, button in pairs(ScrollingFrame:GetChildren()) do
    if button:IsA("TextButton") then
        button.MouseEnter:Connect(function()
            local tween = TweenService:Create(
                button,
                TweenInfo.new(0.2),
                {Size = UDim2.new(1, 10, 0, 55)}
            )
            tween:Play()
        end)
        
        button.MouseLeave:Connect(function()
            local tween = TweenService:Create(
                button,
                TweenInfo.new(0.2),
                {Size = UDim2.new(1, 0, 0, 50)}
            )
            tween:Play()
        end)
    end
end

-- –ê–Ω–∏–º–∞—Ü–∏—è –æ—Ç–∫—Ä—ã—Ç–∏—è/–∑–∞–∫—Ä—ã—Ç–∏—è –º–µ–Ω—é
local menuVisible = true
ToggleButton.MouseButton1Click:Connect(function()
    menuVisible = not menuVisible
    
    if menuVisible then
        local tween = TweenService:Create(
            MainFrame,
            TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out),
            {Position = UDim2.new(0.5, -200, 0.5, -250)}
        )
        tween:Play()
        ToggleButton.Visible = false
    else
        local tween = TweenService:Create(
            MainFrame,
            TweenInfo.new(0.5, Enum.EasingStyle.Back, Enum.EasingDirection.In),
            {Position = UDim2.new(0.5, -200, 1.5, 0)}
        )
        tween:Play()
        ToggleButton.Visible = true
    end
end)

-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è –Ω–∞—Å—Ç—Ä–æ–π–∫–∞ —Å–ø–∏—Å–∫–∞
wait()
ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, UIListLayout.AbsoluteContentSize.Y + 20)

-- –°–∏—Å—Ç–µ–º–∞ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–π
local function showNotification(text, color)
    local notification = Instance.new("TextLabel")
    notification.Size = UDim2.new(0, 300, 0, 50)
    notification.Position = UDim2.new(0.5, -150, 1, 10)
    notification.BackgroundColor3 = color
    notification.Text = "üîî " .. text
    notification.TextColor3 = Color3.white
    notification.Font = Enum.Font.GothamBold
    notification.TextSize = 18
    notification.Parent = ScreenGui
    
    local tweenIn = TweenService:Create(
        notification,
        TweenInfo.new(0.5),
        {Position = UDim2.new(0.5, -150, 0.9, -60)}
    )
    tweenIn:Play()
    
    wait(3)
    
    local tweenOut = TweenService:Create(
        notification,
        TweenInfo.new(0.5),
        {Position = UDim2.new(0.5, -150, 1, 10)}
    )
    tweenOut:Play()
    
    tweenOut.Completed:Connect(function()
        notification:Destroy()
    end)
end

-- –ó–∞–ø—É—Å–∫ —Å–∏—Å—Ç–µ–º—ã
showNotification("STIDEV Cheats v" .. version .. " –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω—ã!", Color3.fromRGB(138, 43, 226))

-- –§–æ–Ω–æ–≤–∞—è –º—É–∑—ã–∫–∞ (–æ–ø—Ü–∏–æ–Ω–∞–ª—å–Ω–æ)
local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://9114828336" -- –≠–ª–µ–∫—Ç—Ä–æ–Ω–Ω–∞—è –º—É–∑—ã–∫–∞
sound.Volume = 0.3
sound.Looped = true
sound.Parent = ScreenGui
sound:Play()

-- –ê–≤—Ç–æ-–ø—Ä–∏–º–µ–Ω–µ–Ω–∏–µ –Ω–∞—Å—Ç—Ä–æ–µ–∫ –ø—Ä–∏ —Ä–µ—Å–ø–∞–≤–Ω–µ
Players.LocalPlayer.CharacterAdded:Connect(function(character)
    wait(1)
    
    if walkspeedValue > 16 then
        local humanoid = character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = walkspeedValue
        end
    end
    
    if godModeEnabled then
        for _, part in pairs(character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
                part.Transparency = 0.3
            end
        end
    end
    
    if espEnabled then
        -- –ü–µ—Ä–µ—Å–æ–∑–¥–∞–Ω–∏–µ ESP –¥–ª—è –Ω–æ–≤—ã—Ö –∏–≥—Ä–æ–∫–æ–≤
        espButton:Click()
        wait(0.1)
        espButton:Click()
    end
end)

