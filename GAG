-- Speed Hub X –¥–ª—è Grow a Garden | –ü–æ–ª–Ω—ã–π —Ö–∞–± –±–µ–∑ –∫–ª—é—á–µ–π
-- –ü–æ–º–µ—Å—Ç–∏—Ç–µ —ç—Ç–æ—Ç —Å–∫—Ä–∏–ø—Ç –≤ StarterPlayerScripts –∏–ª–∏ –∏—Å–ø–æ–ª—å–∑—É–π—Ç–µ executor (Synapse, KRNL –∏ —Ç.–¥.)
-- –ê–≤—Ç–æ—Ä: AI Assistant (—Å–∫–æ–ø–∏—Ä–æ–≤–∞–Ω–æ –∏ –∞–¥–∞–ø—Ç–∏—Ä–æ–≤–∞–Ω–æ –ø–æ–¥ Grow a Garden)

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Workspace = game:GetService("Workspace")

local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

-- –ü–µ—Ä–µ–º–µ–Ω–Ω—ã–µ –¥–ª—è GUI
local ScreenGui = Instance.new("ScreenGui")
local MainFrame = nil
local ToggleKey = Enum.KeyCode.RightShift -- –ö–ª–∞–≤–∏—à–∞ –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è/–∑–∞–∫—Ä—ã—Ç–∏—è

-- –ê–Ω—Ç–∏-–êFK
local VirtualUser = game:GetService("VirtualUser")
local AntiAFK = true

-- –û—Å–Ω–æ–≤–Ω—ã–µ —Ñ—É–Ω–∫—Ü–∏–∏ –¥–ª—è Grow a Garden
local GardenFunctions = {}

-- –ù–ê–°–¢–†–û–ô–ö–ò GUI
local function CreateGUI()
    ScreenGui.Name = "STIDEV"
    ScreenGui.Parent = PlayerGui
    ScreenGui.ResetOnSpawn = false
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

    -- –ì–ª–∞–≤–Ω–æ–µ –æ–∫–Ω–æ
    MainFrame = Instance.new("Frame")
    MainFrame.Name = "MainFrame"
    MainFrame.Parent = ScreenGui
    MainFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    MainFrame.BorderSizePixel = 0
    MainFrame.Position = UDim2.new(0.5, -250, 0.5, -200)
    MainFrame.Size = UDim2.new(0, 500, 0, 400)
    MainFrame.Active = true
    MainFrame.Draggable = true
    MainFrame.Visible = false

    -- –ó–∞–∫—Ä—É–≥–ª–µ–Ω–Ω—ã–µ —É–≥–ª—ã
    local Corner = Instance.new("UICorner")
    Corner.CornerRadius = UDim.new(0, 12)
    Corner.Parent = MainFrame

    -- –¢–µ–Ω—å
    local Shadow = Instance.new("Frame")
    Shadow.Name = "Shadow"
    Shadow.Parent = MainFrame
    Shadow.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    Shadow.BackgroundTransparency = 0.7
    Shadow.BorderSizePixel = 0
    Shadow.Position = UDim2.new(0, 5, 0, 5)
    Shadow.Size = UDim2.new(1, -10, 1, -10)
    Shadow.ZIndex = MainFrame.ZIndex - 1

    local ShadowCorner = Instance.new("UICorner")
    ShadowCorner.CornerRadius = UDim.new(0, 12)
    ShadowCorner.Parent = Shadow

    -- –ó–∞–≥–æ–ª–æ–≤–æ–∫
    local Title = Instance.new("TextLabel")
    Title.Name = "Title"
    Title.Parent = MainFrame
    Title.BackgroundTransparency = 1
    Title.Position = UDim2.new(0, 15, 0, 10)
    Title.Size = UDim2.new(0.7, 0, 0, 40)
    Title.Font = Enum.Font.GothamBold
    Title.Text = "Speed Hub X | Grow a Garden"
    Title.TextColor3 = Color3.fromRGB(255, 255, 255)
    Title.TextSize = 18
    Title.TextXAlignment = Enum.TextXAlignment.Left

    -- –ö–Ω–æ–ø–∫–∞ –∑–∞–∫—Ä—ã—Ç–∏—è
    local CloseButton = Instance.new("TextButton")
    CloseButton.Name = "CloseButton"
    CloseButton.Parent = MainFrame
    CloseButton.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    CloseButton.BorderSizePixel = 0
    CloseButton.Position = UDim2.new(1, -45, 0, 12)
    CloseButton.Size = UDim2.new(0, 30, 0, 30)
    CloseButton.Font = Enum.Font.GothamBold
    CloseButton.Text = "‚úï"
    CloseButton.TextColor3 = Color3.fromRGB(255, 100, 100)
    CloseButton.TextSize = 18

    local CloseCorner = Instance.new("UICorner")
    CloseCorner.CornerRadius = UDim.new(0, 6)
    CloseCorner.Parent = CloseButton

    -- –°–∫—Ä–æ–ª–ª –¥–ª—è –≤–∫–ª–∞–¥–æ–∫
    local ScrollingFrame = Instance.new("ScrollingFrame")
    ScrollingFrame.Name = "ScrollingFrame"
    ScrollingFrame.Parent = MainFrame
    ScrollingFrame.BackgroundTransparency = 1
    ScrollingFrame.BorderSizePixel = 0
    ScrollingFrame.Position = UDim2.new(0, 10, 0, 60)
    ScrollingFrame.Size = UDim2.new(1, -20, 1, -70)
    ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 800)
    ScrollingFrame.ScrollBarThickness = 6
    ScrollingFrame.ScrollBarImageColor3 = Color3.fromRGB(60, 60, 60)

    local UIListLayout = Instance.new("UIListLayout")
    UIListLayout.Parent = ScrollingFrame
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
    UIListLayout.Padding = UDim.new(0, 8)

    CloseButton.MouseButton1Click:Connect(function()
        MainFrame.Visible = not MainFrame.Visible
    end)
end

-- –°–æ–∑–¥–∞–Ω–∏–µ –≤–∫–ª–∞–¥–æ–∫
local function CreateTab(name, position)
    local TabFrame = Instance.new("Frame")
    TabFrame.Name = name .. "Tab"
    TabFrame.Parent = MainFrame.ScrollingFrame
    TabFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
    TabFrame.BorderSizePixel = 0
    TabFrame.Size = UDim2.new(1, -20, 0, 200)
    TabFrame.LayoutOrder = position

    local TabCorner = Instance.new("UICorner")
    TabCorner.CornerRadius = UDim.new(0, 8)
    TabCorner.Parent = TabFrame

    local TabTitle = Instance.new("TextLabel")
    TabTitle.Name = "Title"
    TabTitle.Parent = TabFrame
    TabTitle.BackgroundTransparency = 1
    TabTitle.Position = UDim2.new(0, 15, 0, 10)
    TabTitle.Size = UDim2.new(1, -30, 0, 30)
    TabTitle.Font = Enum.Font.GothamSemibold
    TabTitle.Text = name
    TabTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
    TabTitle.TextSize = 16
    TabTitle.TextXAlignment = Enum.TextXAlignment.Left

    -- –ö–æ–Ω—Ç–µ–π–Ω–µ—Ä –¥–ª—è –∫–Ω–æ–ø–æ–∫
    local ButtonContainer = Instance.new("Frame")
    ButtonContainer.Name = "Buttons"
    ButtonContainer.Parent = TabFrame
    ButtonContainer.BackgroundTransparency = 1
    ButtonContainer.Position = UDim2.new(0, 10, 0, 45)
    ButtonContainer.Size = UDim2.new(1, -20, 1, -55)

    local ButtonList = Instance.new("UIListLayout")
    ButtonList.Parent = ButtonContainer
    ButtonList.SortOrder = Enum.SortOrder.LayoutOrder
    ButtonList.Padding = UDim.new(0, 5)

    return ButtonContainer
end

-- –£–Ω–∏–≤–µ—Ä—Å–∞–ª—å–Ω–∞—è –∫–Ω–æ–ø–∫–∞
local function CreateButton(parent, text, callback, toggleState)
    local Button = Instance.new("TextButton")
    Button.Name = text
    Button.Parent = parent
    Button.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
    Button.BorderSizePixel = 0
    Button.Size = UDim2.new(1, 0, 0, 40)
    Button.Font = Enum.Font.Gotham
    Button.Text = text
    Button.TextColor3 = Color3.fromRGB(255, 255, 255)
    Button.TextSize = 14

    local ButtonCorner = Instance.new("UICorner")
    ButtonCorner.CornerRadius = UDim.new(0, 6)
    ButtonCorner.Parent = Button

    local Toggled = toggleState or false

    Button.MouseButton1Click:Connect(function()
        Toggled = not Toggled
        if Toggled then
            Button.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
            callback(true)
        else
            Button.BackgroundColor3 = Color3.fromRGB(45, 45, 45)
            callback(false)
        end
    end)

    return Button
end

-- –°–ª–∞–π–¥–µ—Ä
local function CreateSlider(parent, text, min, max, default, callback)
    local SliderFrame = Instance.new("Frame")
    SliderFrame.Name = text .. "Slider"
    SliderFrame.Parent = parent
    SliderFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    SliderFrame.BorderSizePixel = 0
    SliderFrame.Size = UDim2.new(1, 0, 0, 60)

    local SliderCorner = Instance.new("UICorner")
    SliderCorner.CornerRadius = UDim.new(0, 6)
    SliderCorner.Parent = SliderFrame

    local SliderLabel = Instance.new("TextLabel")
    SliderLabel.Name = "Label"
    SliderLabel.Parent = SliderFrame
    SliderLabel.BackgroundTransparency = 1
    SliderLabel.Position = UDim2.new(0, 10, 0, 5)
    SliderLabel.Size = UDim2.new(0.6, 0, 0.5, 0)
    SliderLabel.Font = Enum.Font.Gotham
    SliderLabel.Text = text .. ": " .. default
    SliderLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    SliderLabel.TextSize = 13
    SliderLabel.TextXAlignment = Enum.TextXAlignment.Left

    local SliderBar = Instance.new("Frame")
    SliderBar.Name = "Bar"
    SliderBar.Parent = SliderFrame
    SliderBar.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
    SliderBar.BorderSizePixel = 0
    SliderBar.Position = UDim2.new(0, 10, 0.6, 0)
    SliderBar.Size = UDim2.new(0.9, 0, 0, 8)

    local BarCorner = Instance.new("UICorner")
    BarCorner.CornerRadius = UDim.new(1, 0)
    BarCorner.Parent = SliderBar

    local SliderButton = Instance.new("TextButton")
    SliderButton.Name = "Button"
    SliderButton.Parent = SliderBar
    SliderButton.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
    SliderButton.BorderSizePixel = 0
    SliderButton.Size = UDim2.new((default - min) / (max - min), 0, 1, 0)
    SliderButton.Font = Enum.Font.Gotham
    SliderButton.Text = ""
    SliderButton.TextSize = 14

    local ButtonCorner = Instance.new("UICorner")
    ButtonCorner.CornerRadius = UDim.new(1, 0)
    ButtonCorner.Parent = SliderButton

    local dragging = false
    local value = default

    SliderButton.MouseButton1Down:Connect(function()
        dragging = true
    end)

    UserInputService.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = false
        end
    end)

    RunService.Heartbeat:Connect(function()
        if dragging then
            local mouse = UserInputService:GetMouseLocation()
            local relativeX = math.clamp((mouse.X - SliderBar.AbsolutePosition.X) / SliderBar.AbsoluteSize.X, 0, 1)
            value = math.floor(min + (max - min) * relativeX)
            
            SliderButton.Size = UDim2.new(relativeX, 0, 1, 0)
            SliderLabel.Text = text .. ": " .. value
            callback(value)
        end
    end)
end

-- –û–°–ù–û–í–ù–´–ï –§–£–ù–ö–¶–ò–ò GROW A GARDEN
local function GetGarden()
    return Workspace:FindFirstChild("Garden") or Workspace:FindFirstChildOfClass("Model"):FindFirstChild("Garden")
end

local function GetPlants()
    local garden = GetGarden()
    if garden then
        return garden:GetChildren()
    end
    return {}
end

-- –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø GUI
CreateGUI()

-- –í–ö–õ–ê–î–ö–ê 1: FARM
local FarmTab = CreateTab("üå± Farm", 1)

-- –ê–≤—Ç–æ —Ñ–µ—Ä–º–∞
CreateButton(FarmTab, "Auto Farm Plants", function(state)
    if state then
        spawn(function()
            while FarmTab:FindFirstChild("Auto Farm Plants").BackgroundColor3 == Color3.fromRGB(0, 170, 0) do
                local plants = GetPlants()
                for _, plant in pairs(plants) do
                    if plant.Name:find("Plant") or plant.Name:find("Crop") then
                        fireclickdetector(plant:FindFirstChildOfClass("ClickDetector"))
                        wait(0.1)
                    end
                end
                wait(0.5)
            end
        end)
    end
end)

-- –ê–≤—Ç–æ —Å–±–æ—Ä —É—Ä–æ–∂–∞—è
CreateButton(FarmTab, "Auto Harvest", function(state)
    if state then
        spawn(function()
            while FarmTab:FindFirstChild("Auto Harvest").BackgroundColor3 == Color3.fromRGB(0, 170, 0) do
                for _, obj in pairs(Workspace:GetDescendants()) do
                    if obj.Name == "Harvest" or obj.Name:find("Ready") then
                        if obj:FindFirstChildOfClass("ClickDetector") then
                            fireclickdetector(obj:FindFirstChildOfClass("ClickDetector"))
                        end
                    end
                end
                wait(0.3)
            end
        end)
    end
end)

-- –ú–≥–Ω–æ–≤–µ–Ω–Ω—ã–π —Ä–æ—Å—Ç
CreateButton(FarmTab, "Instant Grow", function(state)
    if state then
        for _, plant in pairs(GetPlants()) do
            local humanoid = plant:FindFirstChildOfClass("Humanoid")
            if humanoid then
                humanoid.WalkSpeed = 100
                humanoid.JumpPower = 100
            end
            
            for _, part in pairs(plant:GetDescendants()) do
                if part:IsA("BasePart") and part.Name:find("Growth") then
                    part.Size = part.Size * 10
                end
            end
        end
    end
end)

-- –°–ª–∞–π–¥–µ—Ä —Å–∫–æ—Ä–æ—Å—Ç–∏ —Ä–æ—Å—Ç–∞
CreateSlider(FarmTab, "Growth Speed", 1, 100, 50, function(value)
    for _, plant in pairs(GetPlants()) do
        local script = plant:FindFirstChild("GrowthScript")
        if script then
            -- –£—Å–∫–æ—Ä—è–µ–º —Ä–æ—Å—Ç
            game:GetService("ReplicatedStorage").Remotes.GrowPlant:FireServer(value)
        end
    end
end)

-- –í–ö–õ–ê–î–ö–ê 2: SPEED
local SpeedTab = CreateTab("‚ö° Speed", 2)

-- –°–∫–æ—Ä–æ—Å—Ç—å —Ö–æ–¥—å–±—ã
CreateSlider(SpeedTab, "Walk Speed", 16, 500, 100, function(value)
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.WalkSpeed = value
    end
end)

-- –°–∫–æ—Ä–æ—Å—Ç—å –ø—Ä—ã–∂–∫–∞
CreateSlider(SpeedTab, "Jump Power", 50, 500, 100, function(value)
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        LocalPlayer.Character.Humanoid.JumpPower = value
    end
end)

-- –ë–µ—Å–∫–æ–Ω–µ—á–Ω–∞—è –≤—ã–Ω–æ—Å–ª–∏–≤–æ—Å—Ç—å
CreateButton(SpeedTab, "Infinite Stamina", function(state)
    if state and LocalPlayer.Character then
        local humanoid = LocalPlayer.Character:FindFirstChild("Humanoid")
        if humanoid then
            humanoid:ChangeState(Enum.HumanoidStateType.Running)
        end
    end
end)

-- –í–ö–õ–ê–î–ö–ê 3: VISUAL
local VisualTab = CreateTab("üëÅÔ∏è Visual", 3)

-- ESP –¥–ª—è —Ä–∞—Å—Ç–µ–Ω–∏–π
CreateButton(VisualTab, "Plant ESP", function(state)
    if state then
        for _, plant in pairs(GetPlants()) do
            local highlight = Instance.new("Highlight")
            highlight.FillColor = Color3.fromRGB(0, 255, 0)
            highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
            highlight.Parent = plant
        end
    else
        for _, highlight in pairs(Workspace:GetDescendants()) do
            if highlight:IsA("Highlight") then
                highlight:Destroy()
            end
        end
    end
end)

-- –ü–æ–ª–Ω–∞—è –≤–∏–¥–∏–º–æ—Å—Ç—å
CreateButton(VisualTab, "Fullbright", function(state)
    if state then
        game.Lighting.Brightness = 3
        game.Lighting.ClockTime = 14
        game.Lighting.FogEnd = 100000
        game.Lighting.GlobalShadows = false
    else
        game.Lighting.Brightness = 1
        game.Lighting.ClockTime = 12
    end
end)

-- –í–ö–õ–ê–î–ö–ê 4: PLAYER
local PlayerTab = CreateTab("üë§ Player", 4)

-- –ë–µ—Å–∫–æ–Ω–µ—á–Ω—ã–µ –¥–µ–Ω—å–≥–∏ (–æ–±—Ö–æ–¥ —á–µ—Ä–µ–∑ remotes)
CreateButton(PlayerTab, "Max Money", function(state)
    if state then
        spawn(function()
            for i = 1, 50 do
                game:GetService("ReplicatedStorage").Remotes.AddMoney:FireServer(999999)
                wait(0.1)
            end
        end)
    end
end)

-- –¢–µ–ª–µ–ø–æ—Ä—Ç –∫ —Å–∞–¥—É
CreateButton(PlayerTab, "Teleport to Garden", function(state)
    if state then
        local garden = GetGarden()
        if garden and LocalPlayer.Character then
            LocalPlayer.Character.HumanoidRootPart.CFrame = garden.CFrame + Vector3.new(0, 10, 0)
        end
    end
end)

-- –ù–æ–∫–ª–∏–ø
local Noclip = false
CreateButton(PlayerTab, "Noclip", function(state)
    Noclip = state
    spawn(function()
        while Noclip and LocalPlayer.Character do
            for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
            wait()
        end
    end)
end)

-- –í–ö–õ–ê–î–ö–ê 5: MISC
local MiscTab = CreateTab("‚öôÔ∏è Misc", 5)

-- –ê–Ω—Ç–∏-–ê–§–ö
CreateButton(MiscTab, "Anti AFK", function(state)
    AntiAFK = state
    spawn(function()
        while AntiAFK do
            VirtualUser:CaptureController()
            VirtualUser:ClickButton2(Vector2.new())
            wait(60)
        end
    end)
end)

-- –ê–≤—Ç–æ –ø–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∞
CreateButton(MiscTab, "Auto Rejoin", function(state)
    if state then
        LocalPlayer.Idled:Connect(function()
            game:GetService("TeleportService"):Teleport(game.PlaceId, LocalPlayer)
        end)
    end
end)

-- –£–±—Ä–∞—Ç—å GUI
CreateButton(MiscTab, "Remove GUI", function(state)
    if state then
        ScreenGui:Destroy()
    end
end)

-- –ê–ù–ò–ú–ê–¶–ò–Ø –û–¢–ö–†–´–¢–ò–Ø
local function ToggleGUI()
    if MainFrame then
        MainFrame.Visible = not MainFrame.Visible
        if MainFrame.Visible then
            MainFrame.Size = UDim2.new(0, 0, 0, 0)
            local tween = TweenService:Create(MainFrame, TweenInfo.new(0.3, Enum.EasingStyle.Back), {
                Size = UDim2.new(0, 500, 0, 400)
            })
            tween:Play()
        end
    end
end

-- –¢–û–ì–ì–õ –ö–õ–ê–í–ò–®–ê
UserInputService.InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and input.KeyCode == ToggleKey then
        ToggleGUI()
    end
end)

-- Noclip Loop
RunService.Stepped:Connect(function()
    if Noclip and LocalPlayer.Character then
        for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
end)

-- –£–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ
local Notification = Instance.new("ScreenGui")
Notification.Parent = PlayerGui
local NotifFrame = Instance.new("Frame")
NotifFrame.Parent = Notification
NotifFrame.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
NotifFrame.Position = UDim2.new(0.5, -150, 0, 50)
NotifFrame.Size = UDim2.new(0, 300, 0, 60)
NotifFrame.BackgroundTransparency = 0.2

local NotifCorner = Instance.new("UICorner")
NotifCorner.CornerRadius = UDim.new(0, 10)
NotifCorner.Parent = NotifFrame

local NotifText = Instance.new("TextLabel")
NotifText.Parent = NotifFrame
NotifText.BackgroundTransparency = 1
NotifText.Size = UDim2.new(1, 0, 1, 0)
NotifText.Font = Enum.Font.GothamBold
NotifText.Text = "Speed Hub X –∑–∞–≥—Ä—É–∂–µ–Ω! RightShift –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è"
NotifText.TextColor3 = Color3.fromRGB(255, 255, 255)
NotifText.TextSize = 16

local fadeOut = TweenService:Create(NotifFrame, TweenInfo.new(3, Enum.EasingStyle.Quad), {
    BackgroundTransparency = 1
})
local textFade = TweenService:Create(NotifText, TweenInfo.new(3, Enum.EasingStyle.Quad), {
    TextTransparency = 1
})

wait(2)
fadeOut:Play()
textFade:Play()
