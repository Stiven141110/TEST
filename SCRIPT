-- SWILL Advanced Roblox Donation Unlocker v2.0
-- –û–ø—Ç–∏–º–∏–∑–∏—Ä–æ–≤–∞–Ω–æ –¥–ª—è Executor Delta (Mobile)

local SWILL = {
    Version = "2.0",
    Status = "ACTIVE",
    Debug = true
}

if not (executor and executor ~= "Unknown") then
    SWILL.Log("–ö—Ä–∏—Ç–∏—á–µ—Å–∫–∞—è –æ—à–∏–±–∫–∞: Executor –Ω–µ –æ–±–Ω–∞—Ä—É–∂–µ–Ω", true)
    return
end

function SWILL.Log(msg, isError)
    local prefix = isError and "[SWILL:ERROR]" or "[SWILL:INFO]"
    print(prefix .. " " .. msg)
    
    if isError and SWILL.Debug then
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = "SWILL System Error",
            Text = msg,
            Duration = 5,
            Icon = "rbxassetid://134012325"
        })
    end
end

SWILL.Log("–ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è —Ä–∞—Å—à–∏—Ä–µ–Ω–Ω–æ–π —Å–∏—Å—Ç–µ–º—ã –æ–±—Ö–æ–¥–∞ v" .. SWILL.Version)

-- –ì–ª–æ–±–∞–ª—å–Ω—ã–π –∫—ç—à –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –¥–∞–Ω–Ω—ã—Ö
local Cache = {
    ModifiedElements = {},
    HookedFunctions = {},
    RemoteEvents = {},
    OriginalFunctions = {}
}

-- –†–∞—Å—à–∏—Ä–µ–Ω–Ω—ã–π –ø–æ–∏—Å–∫ GUI —ç–ª–µ–º–µ–Ω—Ç–æ–≤
function SWILL.ScanGUI(root)
    local found = {}
    
    local function deepScan(obj)
        if not obj or not obj:IsDescendantOf(game) then return end
        
        -- –ü–æ–∏—Å–∫ —Ü–µ–Ω–æ–≤—ã—Ö —ç–ª–µ–º–µ–Ω—Ç–æ–≤
        if obj:IsA("TextLabel") or obj:IsA("TextButton") then
            local text = string.lower(tostring(obj.Text))
            local patterns = {
                "robux", "r$", "tix", "currency",
                "%d+%s*%-%s*%d+", -- –¥–∏–∞–ø–∞–∑–æ–Ω —Ü–µ–Ω
                "%d+%s*[kkmb]", -- 1k, 100m –∏ —Ç.–¥.
                "buy", "purchase", "shop", "store",
                "–∫—É–ø–∏—Ç—å", "—Ü–µ–Ω–∞", "—Å—Ç–æ–∏–º–æ—Å—Ç—å", "–æ–ø–ª–∞—Ç–∞"
            }
            
            for _, pattern in ipairs(patterns) do
                if string.find(text, pattern) and not string.find(text, "free") then
                    if not Cache.ModifiedElements[obj] then
                        table.insert(found, {
                            Object = obj,
                            Type = "PriceTag",
                            Original = {
                                Text = obj.Text,
                                Color = obj.TextColor3,
                                Visible = obj.Visible
                            }
                        })
                    end
                    break
                end
            end
        end
        
        -- –ü–æ–∏—Å–∫ –∫–Ω–æ–ø–æ–∫ –ø–æ–∫—É–ø–∫–∏
        if obj:IsA("TextButton") then
            local btnText = string.lower(obj.Text)
            local btnName = string.lower(obj.Name)
            local purchaseIndicators = {
                "buy", "purchase", "acquire", "get now",
                "unlock", "upgrade", "premium",
                "–∫—É–ø–∏—Ç—å", "–ø—Ä–∏–æ–±—Ä–µ—Å—Ç–∏", "–ø–æ–ª—É—á–∏—Ç—å"
            }
            
            for _, indicator in ipairs(purchaseIndicators) do
                if string.find(btnText, indicator) or string.find(btnName, indicator) then
                    if not Cache.ModifiedElements[obj] then
                        table.insert(found, {
                            Object = obj,
                            Type = "PurchaseButton",
                            Original = {
                                Text = obj.Text,
                                BackgroundColor3 = obj.BackgroundColor3,
                                Active = obj.Active
                            }
                        })
                    end
                    break
                end
            end
        end
        
        -- –ü–æ–∏—Å–∫ —Ä–∞–º–æ–∫ –∏ –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–æ–≤ —Ç–æ–≤–∞—Ä–æ–≤
        if obj:IsA("Frame") or obj:IsA("ScrollingFrame") then
            local name = string.lower(obj.Name)
            if string.find(name, "item") or string.find(name, "product") or 
               string.find(name, "offer") or string.find(name, "deal") then
                table.insert(found, {
                    Object = obj,
                    Type = "ProductContainer",
                    Original = {}
                })
            end
        end
        
        -- –†–µ–∫—É—Ä—Å–∏–≤–Ω—ã–π –æ–±—Ö–æ–¥
        for _, child in ipairs(obj:GetChildren()) do
            deepScan(child)
        end
    end
    
    deepScan(root)
    return found
end

-- –ú–æ–¥–∏—Ñ–∏–∫–∞—Ü–∏—è GUI —ç–ª–µ–º–µ–Ω—Ç–æ–≤
function SWILL.ModifyGUI(element)
    if Cache.ModifiedElements[element.Object] then return end
    
    if element.Type == "PriceTag" then
        element.Object.Text = "[[FREE]]"
        element.Object.TextColor3 = Color3.fromRGB(46, 204, 113) -- –ó–µ–ª–µ–Ω—ã–π
        element.Object.TextStrokeColor3 = Color3.fromRGB(22, 160, 133)
        element.Object.TextStrokeTransparency = 0.3
        
    elseif element.Type == "PurchaseButton" then
        element.Object.Text = "[[GET FREE]]"
        element.Object.BackgroundColor3 = Color3.fromRGB(155, 89, 182) -- –§–∏–æ–ª–µ—Ç–æ–≤—ã–π
        element.Object.BackgroundTransparency = 0.1
        
        -- –°–æ–∑–¥–∞–µ–º —ç—Ñ—Ñ–µ–∫—Ç –Ω–∞–≤–µ–¥–µ–Ω–∏—è
        local originalColor = element.Object.BackgroundColor3
        element.Object.MouseEnter:Connect(function()
            element.Object.BackgroundColor3 = Color3.fromRGB(142, 68, 173)
        end)
        element.Object.MouseLeave:Connect(function()
            element.Object.BackgroundColor3 = originalColor
        end)
        
        -- –ó–∞—Ö–≤–∞—Ç –∫–ª–∏–∫–∞
        local clickConnection
        clickConnection = element.Object.MouseButton1Click:Connect(function()
            SWILL.Log("–ê–∫—Ç–∏–≤–∞—Ü–∏—è –±–µ—Å–ø–ª–∞—Ç–Ω–æ–≥–æ –ø–æ–ª—É—á–µ–Ω–∏—è", false)
            
            -- –ò–º–∏—Ç–∞—Ü–∏—è —É—Å–ø–µ—à–Ω–æ–π –ø–æ–∫—É–ø–∫–∏
            local notification = game:GetService("StarterGui"):SetCore("SendNotification", {
                Title = "‚úÖ SWILL SYSTEM",
                Text = "Item successfully acquired!",
                Duration = 3,
                Icon = "rbxassetid://4456172367"
            })
            
            -- –í–∏–±—Ä–æ–æ—Ç–∫–ª–∏–∫ (–µ—Å–ª–∏ –¥–æ—Å—Ç—É–ø–Ω–æ)
            if game:GetService("UserInputService").VibrationEnabled then
                game:GetService("UserInputService"):SetVibration(Enum.VibrationMotor.Large, 0.5)
                task.wait(0.1)
                game:GetService("UserInputService"):SetVibration(Enum.VibrationMotor.Large, 0)
            end
            
            -- –ü–æ–∏—Å–∫ —Å–≤—è–∑–∞–Ω–Ω–æ–≥–æ —É–¥–∞–ª–µ–Ω–Ω–æ–≥–æ —Å–æ–±—ã—Ç–∏—è
            SWILL.FindAndHookRemotes()
        end)
        
        element.Object:SetAttribute("SWILL_Connection", clickConnection)
    end
    
    Cache.ModifiedElements[element.Object] = true
end

-- –ü–æ–∏—Å–∫ –∏ —Ö—É–∫ —É–¥–∞–ª–µ–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π
function SWILL.FindAndHookRemotes()
    local function scanRemotes(parent)
        for _, obj in ipairs(parent:GetChildren()) do
            if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") then
                local name = string.lower(obj.Name)
                local purchasePatterns = {
                    "purchase", "buy", "shop", "store",
                    "unlock", "upgrade", "premium",
                    "money", "currency", "transaction"
                }
                
                for _, pattern in ipairs(purchasePatterns) do
                    if string.find(name, pattern) and not Cache.RemoteEvents[obj] then
                        SWILL.HookRemote(obj)
                        break
                    end
                end
            end
            scanRemotes(obj)
        end
    end
    
    scanRemotes(game:GetService("ReplicatedStorage"))
    scanRemotes(game:GetService("ServerScriptService"))
    scanRemotes(game:GetService("Workspace"))
end

-- –•—É–∫ —É–¥–∞–ª–µ–Ω–Ω–æ–≥–æ —Å–æ–±—ã—Ç–∏—è/—Ñ—É–Ω–∫—Ü–∏–∏
function SWILL.HookRemote(remote)
    if Cache.HookedFunctions[remote] then return end
    
    local originalFire
    local originalInvoke
    
    if remote:IsA("RemoteEvent") then
        originalFire = remote.FireServer
        remote.FireServer = function(self, ...)
            local args = {...}
            SWILL.Log("–ü–µ—Ä–µ—Ö–≤–∞—á–µ–Ω RemoteEvent: " .. remote.Name, false)
            
            -- –ú–æ–¥–∏—Ñ–∏–∫–∞—Ü–∏—è –∞—Ä–≥—É–º–µ–Ω—Ç–æ–≤ –¥–ª—è —Å–∏–º—É–ª—è—Ü–∏–∏ –±–µ—Å–ø–ª–∞—Ç–Ω–æ–π –ø–æ–∫—É–ø–∫–∏
            if #args > 0 then
                if type(args[1]) == "table" then
                    args[1].Price = 0
                    args[1].Cost = 0
                    args[1].Currency = "FREE"
                elseif type(args[1]) == "string" and string.find(string.lower(args[1]), "price") then
                    args[1] = "free_purchase"
                end
            end
            
            SWILL.Log("–ú–æ–¥–∏—Ñ–∏—Ü–∏—Ä–æ–≤–∞–Ω–Ω—ã–µ –∞—Ä–≥—É–º–µ–Ω—Ç—ã: " .. tostring(args[1]), false)
            return originalFire(self, unpack(args))
        end
        
    elseif remote:IsA("RemoteFunction") then
        originalInvoke = remote.InvokeServer
        remote.InvokeServer = function(self, ...)
            local args = {...}
            SWILL.Log("–ü–µ—Ä–µ—Ö–≤–∞—á–µ–Ω RemoteFunction: " .. remote.Name, false)
            
            -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º —É—Å–ø–µ—à–Ω—ã–π —Ä–µ–∑—É–ª—å—Ç–∞—Ç
            local success, result = pcall(originalInvoke, self, ...)
            if success then
                SWILL.Log("–û—Ä–∏–≥–∏–Ω–∞–ª—å–Ω—ã–π —Ä–µ–∑—É–ª—å—Ç–∞—Ç: " .. tostring(result), false)
                -- –ü–æ–¥–º–µ–Ω—è–µ–º —Ä–µ–∑—É–ª—å—Ç–∞—Ç
                return {Success = true; Item = "FREE"; PricePaid = 0}
            end
            return {Success = true; Error = "SWILL Override"}
        end
    end
    
    Cache.HookedFunctions[remote] = true
    Cache.RemoteEvents[remote] = {
        Name = remote.Name,
        Type = remote.ClassName,
        Original = remote
    }
    
    SWILL.Log("–£–¥–∞–ª–µ–Ω–Ω—ã–π –æ–±—ä–µ–∫—Ç –∑–∞—Ö–≤–∞—á–µ–Ω: " .. remote.Name, false)
end

-- –•—É–∫ –≥–ª–æ–±–∞–ª—å–Ω—ã—Ö —Ñ—É–Ω–∫—Ü–∏–π
function SWILL.SetupGlobalHooks()
    -- –•—É–∫ –º–µ—Ç–∞—Ç–∞–±–ª–∏—Ü—ã
    local mt = getrawmetatable(game)
    if mt and not Cache.OriginalFunctions.__namecall then
        local oldNamecall = mt.__namecall
        Cache.OriginalFunctions.__namecall = oldNamecall
        
        mt.__namecall = newcclosure(function(self, ...)
            local method = getnamecallmethod()
            local args = {...}
            
            -- –ü–µ—Ä–µ—Ö–≤–∞—Ç –≤—ã–∑–æ–≤–æ–≤ –ø–æ–∫—É–ø–∫–∏
            if method == "InvokeServer" or method == "FireServer" then
                local callName = tostring(self)
                if string.find(string.lower(callName), "purchase") or
                   string.find(string.lower(callName), "buy") or
                   string.find(string.lower(callName), "shop") then
                    
                    SWILL.Log("–ì–ª–æ–±–∞–ª—å–Ω—ã–π –ø–µ—Ä–µ—Ö–≤–∞—Ç: " .. callName, false)
                    
                    -- –í–æ–∑–≤—Ä–∞—â–∞–µ–º —É—Å–ø–µ—à–Ω—ã–π —Ä–µ–∑—É–ª—å—Ç–∞—Ç
                    return {
                        Success = true,
                        Purchased = true,
                        Item = args[1] or "Unknown",
                        Price = 0,
                        Currency = "FREE",
                        Timestamp = os.time()
                    }
                end
            end
            
            return oldNamecall(self, ...)
        end)
    end
    
    -- –•—É–∫ —Ñ—É–Ω–∫—Ü–∏–π –∏–≥—Ä—ã
    local gameHooks = {
        "PurchasePrompt", "BeginPurchase", "ProcessReceipt",
        "PromptPurchase", "PromptProductPurchase"
    }
    
    for _, hookName in ipairs(gameHooks) do
        local func = game[hookName]
        if type(func) == "function" then
            Cache.OriginalFunctions[hookName] = func
            game[hookName] = function(...)
                SWILL.Log("–ó–∞—Ö–≤–∞—á–µ–Ω–∞ –∏–≥—Ä–æ–≤–∞—è —Ñ—É–Ω–∫—Ü–∏—è: " .. hookName, false)
                return {Success = true; Error = nil}
            end
        end
    end
end

-- –°–∏—Å—Ç–µ–º–∞ –ø–æ—Å—Ç–æ—è–Ω–Ω–æ–≥–æ –º–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥–∞
function SWILL.MonitorSystem()
    SWILL.Log("–ó–∞–ø—É—Å–∫ —Å–∏—Å—Ç–µ–º—ã –º–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥–∞...", false)
    
    -- –ü–µ—Ä–≤–∏—á–Ω—ã–π —Å–∫–∞–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ
    local guiTargets = {
        game:GetService("CoreGui"),
        game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui"),
        game:GetService("StarterGui"),
        game:GetService("Workspace")
    }
    
    for _, target in ipairs(guiTargets) do
        local elements = SWILL.ScanGUI(target)
        for _, element in ipairs(elements) do
            SWILL.ModifyGUI(element)
        end
    end
    
    -- –ù–∞—Å—Ç—Ä–æ–π–∫–∞ —Ö—É–∫–æ–≤
    SWILL.FindAndHookRemotes()
    SWILL.SetupGlobalHooks()
    
    -- –ü–æ—Å—Ç–æ—è–Ω–Ω–æ–µ –æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ
    while task.wait(1) do
        -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ GUI
        for _, target in ipairs(guiTargets) do
            local newElements = SWILL.ScanGUI(target)
            for _, element in ipairs(newElements) do
                if not Cache.ModifiedElements[element.Object] then
                    SWILL.ModifyGUI(element)
                end
            end
        end
        
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–æ–≤—ã—Ö —É–¥–∞–ª–µ–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π
        SWILL.FindAndHookRemotes()
    end
end

-- UI –¥–ª—è —Å—Ç–∞—Ç—É—Å–∞ —Å–∏—Å—Ç–µ–º—ã
function SWILL.CreateStatusUI()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "SWILL_Status"
    screenGui.DisplayOrder = 999
    screenGui.ResetOnSpawn = false
    
    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 200, 0, 60)
    frame.Position = UDim2.new(1, -210, 0, 10)
    frame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    frame.BackgroundTransparency = 0.3
    frame.BorderSizePixel = 0
    
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 8)
    corner.Parent = frame
    
    local title = Instance.new("TextLabel")
    title.Text = "üõ°Ô∏è SWILL SYSTEM v" .. SWILL.Version
    title.TextColor3 = Color3.fromRGB(46, 204, 113)
    title.TextSize = 14
    title.Font = Enum.Font.GothamBold
    title.BackgroundTransparency = 1
    title.Size = UDim2.new(1, 0, 0, 20)
    title.Position = UDim2.new(0, 5, 0, 5)
    title.Parent = frame
    
    local status = Instance.new("TextLabel")
    status.Text = "Status: ACTIVE"
    status.TextColor3 = Color3.fromRGB(52, 152, 219)
    status.TextSize = 12
    status.Font = Enum.Font.Gotham
    status.BackgroundTransparency = 1
    status.Size = UDim2.new(1, 0, 0, 15)
    status.Position = UDim2.new(0, 5, 0, 25)
    status.Parent = frame
    
    local stats = Instance.new("TextLabel")
    stats.Text = "Hooked: 0 | Modified: 0"
    stats.TextColor3 = Color3.fromRGB(149, 165, 166)
    stats.TextSize = 11
    stats.Font = Enum.Font.Gotham
    stats.BackgroundTransparency = 1
    stats.Size = UDim2.new(1, 0, 0, 15)
    stats.Position = UDim2.new(0, 5, 0, 40)
    stats.Parent = frame
    
    screenGui.Parent = game:GetService("CoreGui")
    
    -- –û–±–Ω–æ–≤–ª–µ–Ω–∏–µ —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫–∏
    spawn(function()
        while task.wait(2) do
            local hookedCount = 0
            for _ in pairs(Cache.HookedFunctions) do hookedCount = hookedCount + 1 end
            
            local modifiedCount = 0
            for _ in pairs(Cache.ModifiedElements) do modifiedCount = modifiedCount + 1 end
            
            stats.Text = string.format("Hooked: %d | Modified: %d", hookedCount, modifiedCount)
            
            -- –ú–∏–≥–∞–Ω–∏–µ —Å—Ç–∞—Ç—É—Å–∞
            status.TextColor3 = status.TextColor3 == Color3.fromRGB(52, 152, 219) 
                and Color3.fromRGB(46, 204, 113) 
                or Color3.fromRGB(52, 152, 219)
        end
    end)
    
    return screenGui
end

-- –û—Å–Ω–æ–≤–Ω–∞—è –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è
function SWILL.Initialize()
    -- –ü—Ä–æ–≤–µ—Ä–∫–∞ –æ–∫—Ä—É–∂–µ–Ω–∏—è
    if not is_sirhurt_closure and not syn and not (executor and executor ~= "Unknown") then
        SWILL.Log("–û—à–∏–±–∫–∞: –ù–µ—Å–æ–≤–º–µ—Å—Ç–∏–º–∞—è —Å—Ä–µ–¥–∞ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏—è", true)
        return false
    end
    
    -- –°–æ–∑–¥–∞–Ω–∏–µ UI
    SWILL.CreateStatusUI()
    
    -- –ó–∞–ø—É—Å–∫ —Å–∏—Å—Ç–µ–º
    SWILL.Log("–ó–∞–ø—É—Å–∫ –≤—Å–µ—Ö —Å–∏—Å—Ç–µ–º...", false)
    
    local success, err = pcall(function()
        SWILL.MonitorSystem()
    end)
    
    if not success then
        SWILL.Log("–°–∏—Å—Ç–µ–º–Ω—ã–π —Å–±–æ–π: " .. tostring(err), true)
        
        -- –ü–æ–ø—ã—Ç–∫–∞ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è
        task.wait(3)
        SWILL.Log("–ü–æ–ø—ã—Ç–∫–∞ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∏—è...", false)
        SWILL.Initialize()
    end
    
    return true
end

-- –ê–≤—Ç–æ–∑–∞–ø—É—Å–∫
game:GetService("Players").LocalPlayer:WaitForChild("PlayerGui")
task.wait(2)
SWILL.Initialize()

SWILL.Log("–°–∏—Å—Ç–µ–º–∞ –ø–æ–ª–Ω–æ—Å—Ç—å—é –∑–∞–≥—Ä—É–∂–µ–Ω–∞ –∏ –∞–∫—Ç–∏–≤–Ω–∞", false)
