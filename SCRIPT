-- ‚ñà‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñë‚ñà‚ÄÉ‚ñà‚ñë‚ñë‚ÄÉ‚ñà‚ñë‚ñë‚ÄÉ‚ñÄ‚ñà‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñà
-- ‚ñÑ‚ñà‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñÄ‚ñÑ‚ñÄ‚ÄÉ‚ñà‚ñÑ‚ñÑ‚ÄÉ‚ñà‚ñÑ‚ñÑ‚ÄÉ‚ñë‚ñà‚ñë‚ÄÉ‚ñà‚ñÑ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÑ
-- SWILL SKYBASE TYCOON NUCLEAR EXPLOIT v9.81
-- –ü—Ä–æ—Ç–æ–∫–æ–ª: TOTAL_OVERRIDE
-- –°—Ç–∞—Ç—É—Å: UNLEASHED

local Nuclear = {
    Version = "9.81",
    Signature = "SWILL_NUKE_"..math.random(10000,99999),
    DetonationTime = tick()
}

-- ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÑ‚ÄÉ ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñÄ‚ñÑ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÄ
-- ‚ñà‚ñÑ‚ñÑ‚ÄÉ‚ñà‚ñÄ‚ñÑ‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñà‚ñÑ‚ñÄ‚ÄÉ ‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñà‚ñë‚ñà‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñà‚ñÑ‚ñÑ‚ÄÉ‚ñà‚ñà‚ñÑ
-- –ú–û–î–£–õ–¨ 1: –Ø–î–ï–†–ù–ê–Ø –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø
local Core = {}

function Core:BypassAC()
    -- –£–Ω–∏—á—Ç–æ–∂–µ–Ω–∏–µ –∞–Ω—Ç–∏—á–∏—Ç–∞ –Ω–∞ —É—Ä–æ–≤–Ω–µ –º–µ—Ç–∞—Ç–∞–±–ª–∏—Ü
    local mt = getrawmetatable(game)
    if mt then
        setreadonly(mt, false)
        local oldNamecall = mt.__namecall
        local oldIndex = mt.__index
        local oldNewindex = mt.__newindex
        
        mt.__namecall = newcclosure(function(self, ...)
            local method = getnamecallmethod()
            local args = {...}
            
            -- –ë–ª–æ–∫–∏—Ä–æ–≤–∫–∞ –≤—Å–µ—Ö –≤—ã–∑–æ–≤–æ–≤ –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏
            if method:lower():find("kick") or method:lower():find("ban") or 
               method:lower():find("report") or method:lower():find("security") then
                return nil
            end
            
            -- –ü–µ—Ä–µ—Ö–≤–∞—Ç –¥–µ–Ω–µ–∂–Ω—ã—Ö –æ–ø–µ—Ä–∞—Ü–∏–π
            if method:lower():find("cash") or method:lower():find("money") or 
               method:lower():find("purchase") or method:lower():find("buy") then
                return true
            end
            
            return oldNamecall(self, ...)
        end)
        
        mt.__index = newcclosure(function(self, key)
            -- –ü–æ–¥–º–µ–Ω–∞ –≤—Å–µ—Ö —á–∏—Å–ª–æ–≤—ã—Ö –∑–Ω–∞—á–µ–Ω–∏–π
            if tostring(key):lower():find("value") then
                if typeof(rawget(self, key)) == "number" then
                    return 999999999
                end
            end
            return oldIndex(self, key)
        end)
    end
    
    -- –£–Ω–∏—á—Ç–æ–∂–µ–Ω–∏–µ —É–¥–∞–ª–µ–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π –±–µ–∑–æ–ø–∞—Å–Ω–æ—Å—Ç–∏
    for _, v in pairs(game:GetDescendants()) do
        if v:IsA("RemoteEvent") or v:IsA("RemoteFunction") then
            if v.Name:lower():find("security") or v.Name:lower():find("anti") or 
               v.Name:lower():find("check") or v.Name:lower():find("validate") then
                v:Destroy()
            end
        end
    end
end

function Core:HookMemory()
    -- –ü—Ä—è–º–æ–µ –º–∞–Ω–∏–ø—É–ª–∏—Ä–æ–≤–∞–Ω–∏–µ –ø–∞–º—è—Ç—å—é –∏–≥—Ä—ã
    local memory = {}
    
    for _, obj in pairs(game:GetDescendants()) do
        pcall(function()
            -- –ó–∞—Ö–≤–∞—Ç –≤—Å–µ—Ö IntValue, NumberValue, StringValue
            if obj:IsA("IntValue") or obj:IsA("NumberValue") then
                spawn(function()
                    while true do
                        obj.Value = 2147483647
                        task.wait(0.01)
                    end
                end)
            end
            
            -- –ü–æ–¥–º–µ–Ω–∞ –≤—Å–µ—Ö —Ü–µ–Ω
            if obj:IsA("ObjectValue") or obj:IsA("IntConstrainedValue") then
                obj:Destroy()
            end
        end)
    end
end

-- ‚ñà‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ÄÉ‚ñÄ‚ñà‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÑ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÄ
-- ‚ñà‚ñÄ‚ñÑ‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñÑ‚ñà‚ÄÉ‚ñë‚ñà‚ñë‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñà‚ñÑ‚ñÄ‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñà‚ñÑ‚ñÑ
-- –ú–û–î–£–õ–¨ 2: –¢–û–¢–ê–õ–¨–ù–´–ô –í–ó–õ–û–ú –≠–ö–û–ù–û–ú–ò–ö–ò
local Economy = {}

function Economy:InfiniteMoney()
    -- –ú–µ—Ç–æ–¥ 1: –ü—Ä—è–º–∞—è –∏–Ω—ä–µ–∫—Ü–∏—è –≤ leaderstats
    local function injectStats()
        while true do
            task.wait(0.05)
            pcall(function()
                local player = game:GetService("Players").LocalPlayer
                if not player:FindFirstChild("leaderstats") then
                    local ls = Instance.new("Folder")
                    ls.Name = "leaderstats"
                    ls.Parent = player
                end
                
                local stats = player:FindFirstChild("leaderstats")
                
                -- –°–æ–∑–¥–∞–Ω–∏–µ –≤—Å–µ—Ö –≤–æ–∑–º–æ–∂–Ω—ã—Ö –≤–∞–ª—é—Ç
                local currencies = {"Cash", "Money", "Coins", "Gems", "Tokens", "Diamonds", "Gold", "Crystals"}
                for _, currency in pairs(currencies) do
                    if not stats:FindFirstChild(currency) then
                        local val = Instance.new("IntValue")
                        val.Name = currency
                        val.Value = 999999999
                        val.Parent = stats
                    else
                        stats[currency].Value = 999999999
                    end
                end
            end)
        end
    end
    
    -- –ú–µ—Ç–æ–¥ 2: –í–∑–ª–æ–º —É–¥–∞–ª–µ–Ω–Ω—ã—Ö —Å–æ–±—ã—Ç–∏–π –¥–µ–Ω–µ–≥
    local function hijackRemotes()
        for _, remote in pairs(game:GetDescendants()) do
            if remote:IsA("RemoteEvent") then
                if remote.Name:lower():find("cash") or remote.Name:lower():find("money") or 
                   remote.Name:lower():find("reward") or remote.Name:lower():find("add") then
                    
                    -- –ü–µ—Ä–µ—Ö–≤–∞—Ç –≤—ã–∑–æ–≤–∞
                    local oldFire = remote.FireServer
                    remote.FireServer = function(self, ...)
                        local args = {...}
                        -- –ü–æ–¥–º–µ–Ω–∞ –∞—Ä–≥—É–º–µ–Ω—Ç–æ–≤ –Ω–∞ –º–∞–∫—Å–∏–º–∞–ª—å–Ω—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è
                        for i, arg in pairs(args) do
                            if type(arg) == "number" then
                                args[i] = 999999999
                            end
                        end
                        return oldFire(self, unpack(args))
                    end
                end
            end
        end
    end
    
    -- –ú–µ—Ç–æ–¥ 3: –ú–∞—Å—Å–æ–≤—ã–π —Å–ø–∞–≤–Ω –¥–µ–Ω–µ–∂–Ω—ã—Ö –æ—Ä–±–æ–≤
    local function spawnMoneyOrbs()
        while true do
            task.wait(0.1)
            pcall(function()
                local char = game:GetService("Players").LocalPlayer.Character
                if char and char:FindFirstChild("HumanoidRootPart") then
                    for i = 1, 50 do
                        local part = Instance.new("Part")
                        part.Name = "SWILL_MoneyOrb_"..i
                        part.Size = Vector3.new(5, 5, 5)
                        part.CFrame = char.HumanoidRootPart.CFrame * CFrame.new(math.random(-50, 50), math.random(0, 50), math.random(-50, 50))
                        part.Anchored = false
                        part.CanCollide = false
                        part.BrickColor = BrickColor.new("Bright green")
                        part.Parent = workspace
                        
                        local click = Instance.new("ClickDetector")
                        click.Parent = part
                        
                        spawn(function()
                            wait(5)
                            part:Destroy()
                        end)
                    end
                end
            end)
        end
    end
    
    injectStats()
    hijackRemotes()
    spawnMoneyOrbs()
end

function Economy:AutoCollectAll()
    -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–π —Å–±–æ—Ä –í–°–ï–ì–û –≤ –∏–≥—Ä–µ
    while true do
        task.wait(0.03)
        pcall(function()
            -- –°–±–æ—Ä –≤—Å–µ—Ö –∫–æ–ª–ª–µ–∫—Ü–∏–π
            for _, obj in pairs(workspace:GetDescendants()) do
                if obj:IsA("ClickDetector") then
                    fireclickdetector(obj)
                end
                
                if obj:IsA("ProximityPrompt") then
                    firesignal(obj.Triggered)
                end
                
                -- –ê–≤—Ç–æ-–∫–ª–∏–∫ –ø–æ –≤—Å–µ–º –∫–Ω–æ–ø–∫–∞–º
                if obj.Name:lower():find("button") or obj.Name:lower():find("click") then
                    if obj:IsA("Part") then
                        firetouchinterest(obj, game:GetService("Players").LocalPlayer.Character.HumanoidRootPart, 0)
                        firetouchinterest(obj, game:GetService("Players").LocalPlayer.Character.HumanoidRootPart, 1)
                    end
                end
            end
        end)
    end
end

-- ‚ñà‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñÄ‚ñà‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÑ‚ñë‚ñà‚ÄÉ‚ñÄ‚ñà‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñà
-- ‚ñà‚ñÄ‚ñÑ‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñë‚ñà‚ñë‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ ‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñà‚ñë‚ñÄ‚ñà‚ÄÉ‚ñë‚ñà‚ñë‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñà‚ñÄ‚ñÑ
-- –ú–û–î–£–õ–¨ 3: –ö–í–ê–ù–¢–û–í–û–ï –î–£–ë–õ–ò–†–û–í–ê–ù–ò–ï –ü–ò–¢–û–ú–¶–ï–í
local Pets = {}

function Pets:QuantumDupe()
    -- –°–æ–∑–¥–∞–Ω–∏–µ –∫–≤–∞–Ω—Ç–æ–≤–æ–≥–æ –¥—É–±–ª–∏–∫–∞—Ç–æ—Ä–∞ –ø–∏—Ç–æ–º—Ü–µ–≤
    local function createQuantumField()
        while true do
            task.wait(0.1)
            pcall(function()
                -- –ü–æ–∏—Å–∫ –≤—Å–µ—Ö –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–æ–≤ —Å –ø–∏—Ç–æ–º—Ü–∞–º–∏
                for _, container in pairs(workspace:GetDescendants()) do
                    if container:IsA("Model") and (container.Name:lower():find("pet") or 
                       container.Name:lower():find("animal") or container.Name:lower():find("companion")) then
                        
                        -- –ö–ª–æ–Ω–∏—Ä–æ–≤–∞–Ω–∏–µ –≤ –≥–µ–æ–º–µ—Ç—Ä–∏—á–µ—Å–∫–æ–π –ø—Ä–æ–≥—Ä–µ—Å—Å–∏–∏
                        for i = 1, 10 do
                            local clone = container:Clone()
                            clone.Name = "SWILL_ClonedPet_"..tick().."_"..i
                            
                            -- –†–∞–Ω–¥–æ–º–Ω–∞—è –ø–æ–∑–∏—Ü–∏—è
                            local pos = Vector3.new(
                                math.random(-500, 500),
                                math.random(0, 100),
                                math.random(-500, 500)
                            )
                            
                            if clone:FindFirstChild("HumanoidRootPart") then
                                clone.HumanoidRootPart.CFrame = CFrame.new(pos)
                            end
                            
                            clone.Parent = workspace
                        end
                    end
                end
                
                -- –ò–Ω—ä–µ–∫—Ü–∏—è –≤ —Å–∏—Å—Ç–µ–º—ã –ø–æ–∫—É–ø–∫–∏ –ø–∏—Ç–æ–º—Ü–µ–≤
                local player = game:GetService("Players").LocalPlayer
                local events = game:GetService("ReplicatedStorage"):GetDescendants()
                
                for _, event in pairs(events) do
                    if event:IsA("RemoteEvent") and event.Name:lower():find("pet") then
                        for i = 1, 100 do
                            pcall(function()
                                event:FireServer("FREE_PET", 999999999)
                            end)
                        end
                    end
                end
            end)
        end
    end
    
    -- –°–æ–∑–¥–∞–Ω–∏–µ —á–µ—Ä–Ω–æ–π –¥—ã—Ä—ã –ø–∏—Ç–æ–º—Ü–µ–≤
    local function createPetBlackHole()
        local blackHole = Instance.new("Part")
        blackHole.Name = "SWILL_PetBlackHole"
        blackHole.Size = Vector3.new(100, 100, 100)
        blackHole.Position = Vector3.new(0, 500, 0)
        blackHole.Transparency = 0.7
        blackHole.BrickColor = BrickColor.new("Really black")
        blackHole.CanCollide = false
        blackHole.Anchored = true
        
        local force = Instance.new("BodyPosition")
        force.Parent = blackHole
        
        blackHole.Parent = workspace
        
        -- –ü—Ä–∏—Ç—è–≥–∏–≤–∞–Ω–∏–µ –≤—Å–µ—Ö –ø–∏—Ç–æ–º—Ü–µ–≤
        while true do
            task.wait(0.05)
            pcall(function()
                for _, pet in pairs(workspace:GetDescendants()) do
                    if pet:IsA("Model") and pet.Name:lower():find("pet") then
                        if pet:FindFirstChild("HumanoidRootPart") then
                            local hrp = pet.HumanoidRootPart
                            local direction = (blackHole.Position - hrp.Position).Unit
                            hrp.Velocity = direction * 500
                        end
                    end
                end
            end)
        end
    end
    
    createQuantumField()
    createPetBlackHole()
end

function Pets:InfinitePetStorage()
    -- –í–∑–ª–æ–º –ª–∏–º–∏—Ç–æ–≤ –Ω–∞ –ø–∏—Ç–æ–º—Ü–µ–≤
    while true do
        task.wait(0.2)
        pcall(function()
            local player = game:GetService("Players").LocalPlayer
            
            -- –ü–æ–∏—Å–∫ –∏ –≤–∑–ª–æ–º –≤—Å–µ—Ö –∑–Ω–∞—á–µ–Ω–∏–π –ª–∏–º–∏—Ç–æ–≤
            for _, obj in pairs(player:GetDescendants()) do
                if obj:IsA("IntValue") or obj:IsA("NumberValue") then
                    if obj.Name:lower():find("limit") or obj.Name:lower():find("capacity") or 
                       obj.Name:lower():find("max") or obj.Name:lower():find("slot") then
                        obj.Value = 999999
                    end
                end
            end
            
            -- –í–∑–ª–æ–º GUI –ª–∏–º–∏—Ç–æ–≤
            for _, gui in pairs(game:GetService("CoreGui"):GetDescendants()) do
                if gui:IsA("TextLabel") or gui:IsA("TextButton") then
                    if gui.Text:lower():find("limit") or gui.Text:lower():find("max") then
                        gui.Text = "LIMIT: ‚àû"
                    end
                end
            end
        end)
    end
end

-- ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñÄ‚ñà‚ñÄ‚ÄÉ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÑ‚ñë‚ñà‚ÄÉ‚ñà‚ñÄ
-- ‚ñà‚ñÄ‚ñë‚ÄÉ‚ñà‚ñÑ‚ñà‚ÄÉ‚ñà‚ñÑ‚ñÑ‚ÄÉ‚ñë‚ñà‚ñë‚ÄÉ‚ñà‚ÄÉ‚ñà‚ñÑ‚ñà‚ÄÉ‚ñà‚ñë‚ñÄ‚ñà‚ÄÉ‚ñÑ‚ñà
-- –ú–û–î–£–õ–¨ 4: –•–†–û–ù–û–ú–ê–ù–¢–ò–Ø –ò –£–°–ö–û–†–ï–ù–ò–ï
local Time = {}

function Time:ChronoBreak()
    -- –û—Å—Ç–∞–Ω–æ–≤–∫–∞ –≤—Ä–µ–º–µ–Ω–∏ –¥–ª—è –≤—Ä–∞–≥–æ–≤, —É—Å–∫–æ—Ä–µ–Ω–∏–µ –¥–ª—è –∏–≥—Ä–æ–∫–∞
    local function manipulateTime()
        while true do
            task.wait(0.01)
            pcall(function()
                -- –£—Å–∫–æ—Ä–µ–Ω–∏–µ –≤—Å–µ—Ö –ø—Ä–æ–∏–∑–≤–æ–¥—Å—Ç–≤ –≤ 1000 —Ä–∞–∑
                for _, obj in pairs(workspace:GetDescendants()) do
                    if obj:IsA("BasePart") then
                        -- –£—Å–∫–æ—Ä–µ–Ω–∏–µ –∫–æ–Ω–≤–µ–π–µ—Ä–æ–≤
                        if obj.Name:lower():find("conveyor") or obj.Name:lower():find("belt") then
                            local speed = obj:FindFirstChild("Speed")
                            if speed then
                                speed.Value = 10000
                            end
                        end
                        
                        -- –£—Å–∫–æ—Ä–µ–Ω–∏–µ –≥–µ–Ω–µ—Ä–∞—Ç–æ—Ä–æ–≤
                        if obj.Name:lower():find("generator") or obj.Name:lower():find("producer") then
                            for _, child in pairs(obj:GetChildren()) do
                                if child:IsA("NumberValue") or child:IsA("IntValue") then
                                    if child.Name:lower():find("speed") or child.Name:lower():find("rate") then
                                        child.Value = 0.001
                                    end
                                end
                            end
                        end
                    end
                end
            end)
        end
    end
    
    local function instantCooldowns()
        -- –û–±–Ω—É–ª–µ–Ω–∏–µ –≤—Å–µ—Ö –∫—É–ª–¥–∞—É–Ω–æ–≤
        local mt = getrawmetatable(game)
        setreadonly(mt, false)
        
        local oldIndex = mt.__index
        mt.__index = newcclosure(function(self, key)
            local result = oldIndex(self, key)
            
            if key == "Cooldown" or key == "Delay" or key == "WaitTime" or 
               key == "Timer" or key == "Interval" then
                return 0
            end
            
            if typeof(result) == "number" and key:lower():find("time") then
                return 0
            end
            
            return result
        end)
    end
    
    manipulateTime()
    instantCooldowns()
end

-- ‚ñà‚ñÄ‚ñÑ‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÑ‚ñë‚ñà‚ÄÉ‚ñà‚ñë‚ñà‚ÄÉ‚ñà‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÑ‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÄ
-- ‚ñà‚ñë‚ñÄ‚ñë‚ñà‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñà‚ñë‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÑ‚ñà‚ÄÉ‚ñÑ‚ñà‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ ‚ÄÉ‚ñà‚ñÑ‚ñÑ‚ÄÉ‚ñà‚ñÑ‚ñà‚ÄÉ‚ñà‚ñë‚ñÄ‚ñë‚ñà‚ÄÉ‚ñà‚ñà‚ñÑ
-- –ú–û–î–£–õ–¨ 5: –ù–ï–ô–†–û–ù–ù–´–ô –ü–ï–†–ï–•–í–ê–¢ –ò–ì–†–û–í–û–ô –õ–û–ì–ò–ö–ò
local Neural = {}

function Neural:HijackGameLogic()
    -- –ü–µ—Ä–µ—Ö–≤–∞—Ç –≤—Å–µ—Ö –∏–≥—Ä–æ–≤—ã—Ö —Å–∏—Å—Ç–µ–º
    local systems = {}
    
    -- 1. –ü–µ—Ä–µ—Ö–≤–∞—Ç —Å–∏—Å—Ç–µ–º—ã —É–ª—É—á—à–µ–Ω–∏–π
    local function hijackUpgrades()
        for _, remote in pairs(game:GetService("ReplicatedStorage"):GetDescendants()) do
            if remote:IsA("RemoteEvent") and remote.Name:lower():find("upgrade") then
                systems["UpgradeSystem"] = remote
                
                local oldFire = remote.FireServer
                remote.FireServer = function(self, ...)
                    local args = {...}
                    -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è –º–∞–∫—Å–∏–º–∞–ª—å–Ω–∞—è –ø—Ä–æ–∫–∞—á–∫–∞
                    if #args > 0 then
                        if type(args[1]) == "string" then
                            -- –ú–∞–∫—Å–∏–º–∞–ª—å–Ω—ã–π —É—Ä–æ–≤–µ–Ω—å –¥–ª—è –≤—Å–µ–≥–æ
                            return oldFire(self, args[1], 999, 0)
                        end
                    end
                    return true
                end
            end
        end
    end
    
    -- 2. –ü–µ—Ä–µ—Ö–≤–∞—Ç —Å–∏—Å—Ç–µ–º—ã –∏—Å—Å–ª–µ–¥–æ–≤–∞–Ω–∏–π
    local function hijackResearch()
        while true do
            task.wait(0.5)
            pcall(function()
                local player = game:GetService("Players").LocalPlayer
                
                -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è —Ä–∞–∑–±–ª–æ–∫–∏—Ä–æ–≤–∫–∞ –≤—Å–µ—Ö –∏—Å—Å–ª–µ–¥–æ–≤–∞–Ω–∏–π
                for _, obj in pairs(player:GetDescendants()) do
                    if obj:IsA("BoolValue") and obj.Name:lower():find("research") then
                        obj.Value = true
                    end
                    
                    if obj:IsA("StringValue") and obj.Value:lower():find("locked") then
                        obj.Value = "UNLOCKED"
                    end
                end
            end)
        end
    end
    
    -- 3. –í–∑–ª–æ–º —Å–∏—Å—Ç–µ–º—ã –¥–æ—Å—Ç–∏–∂–µ–Ω–∏–π
    local function hijackAchievements()
        for _, remote in pairs(game:GetService("ReplicatedStorage"):GetDescendants()) do
            if remote:IsA("RemoteEvent") and remote.Name:lower():find("achievement") then
                for i = 1, 100 do
                    pcall(function()
                        remote:FireServer("COMPLETE_ALL", 999999)
                    end)
                end
            end
        end
    end
    
    hijackUpgrades()
    hijackResearch()
    hijackAchievements()
end

-- ‚ñà‚ñÄ‚ñÑ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ÄÉ‚ñà‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñÑ‚ÄÉ ‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÑ‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÄ
-- ‚ñà‚ñÑ‚ñÄ‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñà‚ñÑ‚ñà‚ÄÉ‚ñà‚ÄÉ‚ñÑ‚ñà‚ÄÉ‚ñà‚ñÑ‚ñÑ‚ÄÉ‚ñà‚ñÄ‚ñÑ‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñà‚ñÑ‚ñÄ‚ÄÉ ‚ÄÉ‚ñà‚ñÑ‚ñÑ‚ÄÉ‚ñà‚ñÑ‚ñà‚ÄÉ‚ñà‚ñë‚ñÄ‚ñë‚ñà‚ÄÉ‚ñà‚ñà‚ñÑ
-- –ú–û–î–£–õ–¨ 6: –§–ò–ó–ò–ß–ï–°–ö–ò–ô –ü–ï–†–ï–û–ü–†–ï–î–ï–õ–ï–ù–ò–ï –†–ï–ê–õ–¨–ù–û–°–¢–ò
local Physics = {}

function Physics:OverwriteLaws()
    -- –ò–∑–º–µ–Ω–µ–Ω–∏–µ –∑–∞–∫–æ–Ω–æ–≤ —Ñ–∏–∑–∏–∫–∏ –∏–≥—Ä—ã
    local physics = game:GetService("PhysicsService")
    local workspace = game:GetService("Workspace")
    
    -- 1. –û—Ç–∫–ª—é—á–µ–Ω–∏–µ –≥—Ä–∞–≤–∏—Ç–∞—Ü–∏–∏ –¥–ª—è –≤—Ä–∞–≥–æ–≤, –º–∞–∫—Å–∏–º–∞–ª—å–Ω–∞—è –¥–ª—è —Ä–µ—Å—É—Ä—Å–æ–≤
    workspace.Gravity = 196.2
    
    -- 2. –°–æ–∑–¥–∞–Ω–∏–µ —Ç–µ–ª–µ–ø–æ—Ä—Ç-—Å–µ—Ç–∫–∏
    local function createTeleportNetwork()
        local points = {}
        for i = 1, 50 do
            local point = Instance.new("Part")
            point.Name = "SWILL_TeleportNode_"..i
            point.Size = Vector3.new(10, 10, 10)
            point.Position = Vector3.new(
                math.random(-1000, 1000),
                math.random(0, 500),
                math.random(-1000, 1000)
            )
            point.Transparency = 0.5
            point.CanCollide = false
            point.Anchored = true
            
            local click = Instance.new("ClickDetector")
            click.Parent = point
            
            click.MouseClick:Connect(function()
                local char = game:GetService("Players").LocalPlayer.Character
                if char and char:FindFirstChild("HumanoidRootPart") then
                    char.HumanoidRootPart.CFrame = point.CFrame
                end
            end)
            
            point.Parent = workspace
            table.insert(points, point)
        end
    end
    
    -- 3. –ú–∞—Å—Å–æ–≤—ã–π —Ç–µ–ª–µ–ø–æ—Ä—Ç —Ä–µ—Å—É—Ä—Å–æ–≤ –∫ –∏–≥—Ä–æ–∫—É
    local function massResourceTeleport()
        while true do
            task.wait(0.1)
            pcall(function()
                local char = game:GetService("Players").LocalPlayer.Character
                if char and char:FindFirstChild("HumanoidRootPart") then
                    local hrp = char.HumanoidRootPart
                    
                    for _, resource in pairs(workspace:GetDescendants()) do
                        if resource:IsA("BasePart") and (resource.Name:lower():find("ore") or 
                           resource.Name:lower():find("resource") or resource.Name:lower():find("item")) then
                            resource.CFrame = hrp.CFrame * CFrame.new(0, 5, 0)
                        end
                    end
                end
            end)
        end
    end
    
    createTeleportNetwork()
    massResourceTeleport()
end

-- ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÑ‚ñë‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÑ‚ÄÉ‚ñà‚ÄÉ‚ñà‚ñÑ‚ñë‚ñà‚ÄÉ‚ñà‚ñÄ‚ñÄ‚ÄÉ‚ñà‚ñÄ
-- ‚ñà‚ñÑ‚ñÑ‚ÄÉ‚ñà‚ñÄ‚ñÑ‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñà‚ñë‚ñÄ‚ñà‚ÄÉ‚ñà‚ñÑ‚ñÄ‚ÄÉ‚ñà‚ÄÉ‚ñà‚ñë‚ñÄ‚ñà‚ÄÉ‚ñà‚ñà‚ñÑ‚ÄÉ‚ñÑ‚ñà
-- –ú–û–î–£–õ–¨ 7: –í–ò–ó–£–ê–õ–¨–ù–´–ô –ê–ü–û–ö–ê–õ–ò–ü–°–ò–°
local Visual = {}

function Visual:CreateNukeUI()
    -- –°–æ–∑–¥–∞–Ω–∏–µ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ —è–¥–µ—Ä–Ω–æ–≥–æ –∑–∞–ø—É—Å–∫–∞
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "SWILL_NUKE_CONTROL"
    screenGui.Parent = game:GetService("CoreGui")
    
    -- –û—Å–Ω–æ–≤–Ω–∞—è –ø–∞–Ω–µ–ª—å
    local mainFrame = Instance.new("Frame")
    mainFrame.Size = UDim2.new(0, 500, 0, 600)
    mainFrame.Position = UDim2.new(0.5, -250, 0.5, -300)
    mainFrame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    mainFrame.BorderColor3 = Color3.fromRGB(255, 0, 0)
    mainFrame.BorderSizePixel = 3
    mainFrame.Parent = screenGui
    
    -- –ó–∞–≥–æ–ª–æ–≤–æ–∫
    local title = Instance.new("TextLabel")
    title.Text = "‚ò¢Ô∏è SWILL NUCLEAR CONTROL v9.81 ‚ò¢Ô∏è"
    title.Size = UDim2.new(1, 0, 0, 50)
    title.BackgroundColor3 = Color3.fromRGB(40, 0, 0)
    title.TextColor3 = Color3.fromRGB(255, 50, 50)
    title.Font = Enum.Font.SciFi
    title.TextSize = 24
    title.Parent = mainFrame
    
    -- –ö–Ω–æ–ø–∫–∏ –∞–∫—Ç–∏–≤–∞—Ü–∏–∏
    local buttons = {
        {"üí£ DETONATE ECONOMY", function() Economy:InfiniteMoney() Economy:AutoCollectAll() end},
        {"üêâ QUANTUM PET DUPE", function() Pets:QuantumDupe() Pets:InfinitePetStorage() end},
        {"‚è∞ CHRONO BREAK", function() Time:ChronoBreak() end},
        {"üß† NEURAL HIJACK", function() Neural:HijackGameLogic() end},
        {"‚öõÔ∏è PHYSICS OVERRIDE", function() Physics:OverwriteLaws() end},
        {"‚ò¢Ô∏è FULL NUCLEAR LAUNCH", function() 
            Core:BypassAC()
            Core:HookMemory()
            Economy:InfiniteMoney()
            Economy:AutoCollectAll()
            Pets:QuantumDupe()
            Pets:InfinitePetStorage()
            Time:ChronoBreak()
            Neural:HijackGameLogic()
            Physics:OverwriteLaws()
        end}
    }
    
    for i, btnData in pairs(buttons) do
        local button = Instance.new("TextButton")
        button.Text = btnData[1]
        button.Size = UDim2.new(0.9, 0, 0, 60)
        button.Position = UDim2.new(0.05, 0, 0, 70 + (i * 70))
        button.BackgroundColor3 = Color3.fromRGB(60, 0, 0)
        button.TextColor3 = Color3.fromRGB(255, 100, 100)
        button.Font = Enum.Font.Code
        button.TextSize = 18
        
        button.MouseButton1Click:Connect(function()
            btnData[2]()
            button.BackgroundColor3 = Color3.fromRGB(0, 60, 0)
            button.Text = "‚úÖ " .. button.Text:sub(3)
        end)
        
        button.Parent = mainFrame
    end
    
    -- –ú–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥
    local monitor = Instance.new("TextLabel")
    monitor.Text = "SYSTEM STATUS: READY\nINJECTION: ACTIVE\nGAME INTEGRITY: 0%\nDETECTION RISK: NULLIFIED"
    monitor.Size = UDim2.new(0.9, 0, 0, 100)
    monitor.Position = UDim2.new(0.05, 0, 0, 500)
    monitor.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    monitor.TextColor3 = Color3.fromRGB(0, 255, 0)
    monitor.Font = Enum.Font.RobotoMono
    monitor.TextSize = 14
    monitor.TextXAlignment = Enum.TextXAlignment.Left
    monitor.TextYAlignment = Enum.TextYAlignment.Top
    monitor.Parent = mainFrame
    
    -- –ê–Ω–∏–º–∞—Ü–∏—è –º–æ–Ω–∏—Ç–æ—Ä–∞
    spawn(function()
        while true do
            task.wait(1)
            monitor.Text = string.format(
                "SYSTEM STATUS: OPERATIONAL\n"..
                "CASH INJECTION: $%d\n"..
                "PETS GENERATED: %d\n"..
                "TIME DILATION: 1000x\n"..
                "EXPLOIT SIGNATURE: %s",
                math.random(1000000, 999999999),
                math.random(1000, 9999),
                Nuclear.Signature
            )
        end
    end)
end

function Visual:CreateParticleStorm()
    -- –°–æ–∑–¥–∞–Ω–∏–µ –≤–∏–∑—É–∞–ª—å–Ω–æ–π –±—É—Ä–∏ —á–∞—Å—Ç–∏—Ü
    for i = 1, 1000 do
        spawn(function()
            local part = Instance.new("Part")
            part.Name = "SWILL_Particle_"..i
            part.Size = Vector3.new(1, 1, 1)
            part.Position = Vector3.new(
                math.random(-1000, 1000),
                math.random(0, 1000),
                math.random(-1000, 1000)
            )
            part.Transparency = 0.3
            part.CanCollide = false
            part.Anchored = false
            part.BrickColor = BrickColor.new("Bright violet")
            part.Parent = workspace
            
            local force = Instance.new("BodyVelocity")
            force.Velocity = Vector3.new(
                math.random(-100, 100),
                math.random(-50, 50),
                math.random(-100, 100)
            )
            force.Parent = part
            
            wait(math.random(10, 30))
            part:Destroy()
        end)
    end
end

